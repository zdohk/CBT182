%# Memlist# Functions# Options# Special# Defaults# Help+
"-------------------------------------------------------------------------------
+\ \¢List Files+\ \
%Command ===>¦ZCMD                                            %Scroll ===>_Z   +
Enter an ISPF command or a special control code:
+  -----------------------------------------------------------------------------
}CMD +C V DATA/MSG DO  RECF LRECL BLKSI$volume$data
)MODEL
_Z   @Z@Z_Z       @Z  @Z   @Z    @Z    @Z     @Z
)INIT
   &OPT3 = 1
   .HELP  = P86PNK@
   .ZVARS = '(ZSCBR PDSCOPT PDSCCAT PDSCVTOC PDSCDATA PDSCDSOR +
            PDSCRF PDSCLREC PDSCBLKS PDSCVOL PDSCDSN)'
   VPUT (PDSTNAME) SHARED
 &volume = 'VOLUME'
 &data   = '---------DATA SET NAME ----------'
)PROC
IF (&ZCMD = 'MLALL')
  &ZCMD = 'MEMLIST :'
IF (&ZCMD = 'MLTO')
  &ZCMD = 'MEMLIST : TODAY'
IF (&ZCMD = 'MLYES')
  &ZCMD = 'MEMLIST : YESTERDAY'
IF (&ZCMD = 'MLWE')
  &ZCMD = 'MEMLIST : WEEK'
IF (&ZCMD = 'MLMON')
  &ZCMD = 'MEMLIST : MONTH'
IF (&ZCMD = 'MLQTR')
  &ZCMD = 'MEMLIST : QUARTER'
IF (&ZCMD = 'MLYR')
  &ZCMD = 'MEMLIST : YEAR'
IF (&ZCMD = 'SORTPDC')
  &ZCMD = 'SORT ?'
IF (&ZCMD = 'FINDPDC')
  &ZCMD = 'F ?'
IF (&ZCMD = 'EXCLALL')
  &ZCMD = 'X ALL'
IF (&ZCMD = 'EXCLUP')
  &ZCMD = 'X UP'
IF (&ZCMD = 'EXCLDN')
  &ZCMD = 'X DN'
IF (&ZCMD = 'N1')
  &ZCMD = 1
IF (&ZCMD = 'N2')
  &ZCMD = 2
IF (&ZCMD = 'N3')
  &ZCMD = 3
IF (&ZCMD = 'N4')
  &ZCMD = 4
IF (&ZCMD = 'N5')
  &ZCMD = 5
IF (&ZCMD = 'N6')
  &ZCMD = 6
IF (&ZCMD = 'N7')
  &ZCMD = 7
IF (&ZCMD = 'N8')
  &ZCMD = 8
IF (&ZCMD = 'N9')
  &ZCMD = 9
IF (&ZCMD = 'VOLUME')
  IF (&ZPSLFX1 = '')
    &ZCMD = 'SORT VOLUME ASCENDING'
    &ZPSLFX1 = ''     /*      VOLUME        */
    &ZPSLFX2 = ''     /*      DSNAME        */
    &ZPSLFX3 = ''     /*      CDATE         */
    &ZPSLFX4 = ''     /*      SIZE          */
    &ZPSLFX5 = ''     /*      RDATE         */
    &ZPSLFX1 = 1
  ELSE
    &ZCMD = 'SORT VOLUME DESCENDING'
    &ZPSLFX1 = ''
IF (&ZCMD = 'DSNAME')
  IF (&ZPSLFX2 = '')
    &ZCMD = 'SORT DSNAME ASCENDING'
    &ZPSLFX1 = ''     /*      VOLUME        */
    &ZPSLFX2 = ''     /*      DSNAME        */
    &ZPSLFX3 = ''     /*      CDATE         */
    &ZPSLFX4 = ''     /*      SIZE          */
    &ZPSLFX5 = ''     /*      RDATE         */
    &ZPSLFX2 = 1
  ELSE
    &ZCMD = 'SORT DSNAME DESCENDING'
    &ZPSLFX2 = ''
)PNTS
FIELD(volume) VAR(ZCMD) VAL('SORT VOLUME')
FIELD(data)   VAR(ZCMD) VAL('SORT DSNAME')
)END
