  IF (&ZPSLVX1 = '')
    &ZCMD = 'SORT VOLUME ASCENDING'
    &ZPSLVX1 = ''     /*      VOLUME        */
    &ZPSLVX2 = ''     /*      ADDRESS       */
    &ZPSLVX3 = ''     /*      DEVTYPE       */
    &ZPSLVX4 = ''     /*      COUNT         */
    &ZPSLVX5 = ''     /*      TOTAL         */
    &ZPSLVX6 = ''     /*      LARGEST       */
    &ZPSLVX7 = ''     /*      USED          */
    &ZPSLVX1 = 1
  ELSE
    &ZCMD = 'SORT VOLUME DESCENDING'
    &ZPSLVX1 = ''
IF (&ZCMD = 'ADDRESS')
  IF (&ZPSLVX2 = '')
    &ZCMD = 'SORT ADDRESS ASCENDING'
    &ZPSLVX1 = ''     /*      VOLUME        */
    &ZPSLVX2 = ''     /*      ADDRESS       */
    &ZPSLVX3 = ''     /*      DEVTYPE       */
    &ZPSLVX4 = ''     /*      COUNT         */
    &ZPSLVX5 = ''     /*      TOTAL         */
    &ZPSLVX6 = ''     /*      LARGEST       */
    &ZPSLVX7 = ''     /*      USED          */
    &ZPSLVX2 = 1
  ELSE
    &ZCMD = 'SORT ADDRESS DESCENDING'
    &ZPSLVX2 = ''
IF (&ZCMD = 'TYPE')
  IF (&ZPSLVX3 = '')
    &ZCMD = 'SORT TYPE ASCENDING'
    &ZPSLVX1 = ''     /*      VOLUME        */
    &ZPSLVX2 = ''     /*      ADDRESS       */
    &ZPSLVX3 = ''     /*      DEVTYPE       */
    &ZPSLVX4 = ''     /*      COUNT         */
    &ZPSLVX5 = ''     /*      TOTAL         */
    &ZPSLVX6 = ''     /*      LARGEST       */
    &ZPSLVX7 = ''     /*      USED          */
    &ZPSLVX3 = 1
  ELSE
    &ZCMD = 'SORT TYPE DESCENDING'
    &ZPSLVX3 = ''
IF (&ZCMD = 'COUNT')
  IF (&ZPSLVX4 = '')
    &ZCMD = 'SORT COUNT ASCENDING'
    &ZPSLVX1 = ''     /*      VOLUME        */
    &ZPSLVX2 = ''     /*      ADDRESS       */
    &ZPSLVX3 = ''     /*      DEVTYPE       */
    &ZPSLVX4 = ''     /*      COUNT         */
    &ZPSLVX5 = ''     /*      TOTAL         */
    &ZPSLVX6 = ''     /*      LARGEST       */
    &ZPSLVX7 = ''     /*      USED          */
    &ZPSLVX4 = 1
  ELSE
    &ZCMD = 'SORT COUNT DESCENDING'
    &ZPSLVX4 = ''
IF (&ZCMD = 'USED')
  IF (&ZPSLVX7 = '')
    &ZCMD = 'SORT USED ASCENDING'
    &ZPSLVX1 = ''     /*      VOLUME        */
    &ZPSLVX2 = ''     /*      ADDRESS       */
    &ZPSLVX3 = ''     /*      DEVTYPE       */
    &ZPSLVX4 = ''     /*      COUNT         */
    &ZPSLVX5 = ''     /*      TOTAL         */
    &ZPSLVX6 = ''     /*      LARGEST       */
    &ZPSLVX7 = ''     /*      USED          */
    &ZPSLVX7 = 1
  ELSE
