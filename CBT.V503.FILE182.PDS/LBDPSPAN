)BODY EXPAND(\\)
%# Memlist# Functions# Options# Special# Defaults# Help+
"-------------------------------------------------------------------------------
+\ \¢MEMLIST Load Member List 3+\ \
%Command ===>¦ZCMD                                            %Scroll ===>_Z   +
Enter an ISPF command a PDS subcommand or a special control code:
%&PDSMSG  ----------------------------------------------------------------------
}CMD $name    }DATA    +ALIASOF $lenlked +  -- $attr      +  --$ac$mode+$main+
+                       MATCH   $length  $ttr      +ENTRY     SSI
)MODEL
%---- -------- -------- -------- -------- ---- ---- ---- ---- ---- ---- --------
_Z   ¬Z       _Z       @Z       @Z         @Z   @Z   @Z   @Z    @Z@Z   @Z
+                      @Z       @Z       @Z        @Z        @Z        +
)INIT
   &OPT3 = 1
   .NRET = ON
   .HELP  = P86PND@
   .ZVARS = '(ZSCBR MEMLOPT  MEMLNAME MEMLDATA MEMLALIS MEMLLDAX +
            MEMLATT1 MEMLATT2 MEMLDC   MEMLTEST MEMLAPFX MEMLMODE MEMLMNAM +
            MEMLMTTR MEMLLENG MEMLTTR MEMLENTY MEMLSSI)'
 &name    = 'NAME'
 &lenlked = 'LEN/LKED'
 &attr    = 'ATTRIBUTES'
 &ac      = 'AC'
 &mode    = 'MODE'
 &main    = 'MAIN'
 &length  = 'LENGTH'
 &ttr     = 'TTR'
)REINIT
   .NRET = ON
   REFRESH(*)
)PROC
IF (&ZCMD = 'MLALL')
  &ZCMD = 'MEMLIST :'
IF (&ZCMD = 'MLTO')
  &ZCMD = 'MEMLIST : TODAY'
IF (&ZCMD = 'MLYES')
  &ZCMD = 'MEMLIST : YESTERDAY'
IF (&ZCMD = 'MLWE')
  &ZCMD = 'MEMLIST : WEEK'
IF (&ZCMD = 'MLMON')
  &ZCMD = 'MEMLIST : MONTH'
IF (&ZCMD = 'MLQTR')
  &ZCMD = 'MEMLIST : QUARTER'
IF (&ZCMD = 'MLYR')
  &ZCMD = 'MEMLIST : YEAR'
IF (&ZCMD = 'SORTPDC')
  &ZCMD = 'SORT ?'
IF (&ZCMD = 'FINDPDC')
  &ZCMD = 'F ?'
IF (&ZCMD = 'CONTAPDC')
  &ZCMD = 'CONTROL ALL'
IF (&ZCMD = 'USAGAPDC')
  &ZCMD = 'USAGE ALL'
IF (&ZCMD = 'CONTGPDC')
  &ZCMD = 'CONTROL GEO'
IF (&ZCMD = 'EXCLALL')
  &ZCMD = 'X ALL'
IF (&ZCMD = 'EXCLUP')
  &ZCMD = 'X UP'
IF (&ZCMD = 'EXCLDN')
  &ZCMD = 'X DN'
IF (&ZCMD = 'CHGMENU')
  &ZCMD = 'O.UT.CHGMENU'
IF (&ZCMD = 'N1')
  &ZCMD = 1
IF (&ZCMD = 'N2')
  &ZCMD = 2
IF (&ZCMD = 'N3')
  &ZCMD = 3
IF (&ZCMD = 'N4')
  &ZCMD = 4
IF (&ZCMD = 'N5')
  &ZCMD = 5
IF (&ZCMD = 'N6')
  &ZCMD = 6
IF (&ZCMD = 'N7')
  &ZCMD = 7
IF (&ZCMD = 'N8')
  &ZCMD = 8
IF (&ZCMD = 'N9')
  &ZCMD = 9
IF (&ZCMD = 'NAME')
  IF (&ZPSMLL1 = '')
    &ZCMD = 'SORT NAME ASCENDING'
    &ZPSMLL1 = ''     /*      NAME          */
    &ZPSMLL2 = ''     /*      CREATED       */
    &ZPSMLL3 = ''     /*      LENGTH        */
    &ZPSMLL4 = ''     /*      TTR           */
    &ZPSMLL5 = ''     /*      ATTR          */
    &ZPSMLL6 = ''     /*      AUTH          */
    &ZPSMLL7 = ''     /*      MAIN          */
    &ZPSMLL8 = ''     /*      MODE          */
    &ZPSMLL1 = 1
  ELSE
    &ZCMD = 'SORT NAME DESCENDING'
    &ZPSMLL1 = ''
IF (&ZCMD = 'CREATED')
  IF (&ZPSMLL2 = '')
    &ZCMD = 'SORT LENLKED  DESCENDING'
    &ZPSMLL1 = ''     /*      NAME          */
    &ZPSMLL2 = ''     /*      CREATED       */
    &ZPSMLL3 = ''     /*      LENGTH        */
    &ZPSMLL4 = ''     /*      TTR           */
    &ZPSMLL5 = ''     /*      ATTR          */
    &ZPSMLL6 = ''     /*      AUTH          */
    &ZPSMLL7 = ''     /*      MAIN          */
    &ZPSMLL8 = ''     /*      MODE          */
    &ZPSMLL2 = 1
  ELSE
    &ZCMD = 'SORT LENLKED ASCENDING'
    &ZPSMLL2 = ''
IF (&ZCMD = 'LENGTH')
  IF (&ZPSMLL3 = '')
    &ZCMD = 'SORT LENGTH DESCENDING'
    &ZPSMLL1 = ''     /*      NAME          */
    &ZPSMLL2 = ''     /*      CREATED       */
    &ZPSMLL3 = ''     /*      LENGTH        */
    &ZPSMLL4 = ''     /*      TTR           */
    &ZPSMLL5 = ''     /*      ATTR          */
    &ZPSMLL6 = ''     /*      AUTH          */
    &ZPSMLL7 = ''     /*      MAIN          */
    &ZPSMLL8 = ''     /*      MODE          */
    &ZPSMLL3 = 1
  ELSE
    &ZCMD = 'SORT LENGTH ASCENDING'
    &ZPSMLL3 = ''
IF (&ZCMD = 'TTR')
  IF (&ZPSMLL4 = '')
    &ZCMD = 'SORT TTR ASCENDING'
    &ZPSMLL1 = ''     /*      NAME          */
    &ZPSMLL2 = ''     /*      CREATED       */
    &ZPSMLL3 = ''     /*      LENGTH        */
    &ZPSMLL4 = ''     /*      TTR           */
    &ZPSMLL5 = ''     /*      ATTR          */
    &ZPSMLL6 = ''     /*      AUTH          */
    &ZPSMLL7 = ''     /*      MAIN          */
    &ZPSMLL8 = ''     /*      MODE          */
    &ZPSMLL4 = 1
  ELSE
    &ZCMD = 'SORT TTR DESCENDING'
    &ZPSMLL4 = ''
IF (&ZCMD = 'LMATTR')
  IF (&ZPSMLL5 = '')
    &ZCMD = 'SORT ATTR ASCENDING'
    &ZPSMLL1 = ''     /*      NAME          */
    &ZPSMLL2 = ''     /*      CREATED       */
    &ZPSMLL3 = ''     /*      LENGTH        */
    &ZPSMLL4 = ''     /*      TTR           */
    &ZPSMLL5 = ''     /*      ATTR          */
    &ZPSMLL6 = ''     /*      AUTH          */
    &ZPSMLL7 = ''     /*      MAIN          */
    &ZPSMLL8 = ''     /*      MODE          */
    &ZPSMLL5 = 1
  ELSE
    &ZCMD = 'SORT ATTR DESCENDING'
    &ZPSMLL5 = ''
IF (&ZCMD = 'AUTH')
  IF (&ZPSMLL6 = '')
    &ZCMD = 'SORT AUTH ASCENDING'
    &ZPSMLL1 = ''     /*      NAME          */
    &ZPSMLL2 = ''     /*      CREATED       */
    &ZPSMLL3 = ''     /*      LENGTH        */
    &ZPSMLL4 = ''     /*      TTR           */
    &ZPSMLL5 = ''     /*      ATTR          */
    &ZPSMLL6 = ''     /*      AUTH          */
    &ZPSMLL7 = ''     /*      MAIN          */
    &ZPSMLL8 = ''     /*      MODE          */
    &ZPSMLL6 = 1
  ELSE
    &ZCMD = 'SORT AUTH DESCENDING'
    &ZPSMLL6 = ''
IF (&ZCMD = 'MAIN')
  IF (&ZPSMLL7 = '')
    &ZCMD = 'SORT MAIN ASCENDING'
    &ZPSMLL1 = ''     /*      NAME          */
    &ZPSMLL2 = ''     /*      CREATED       */
    &ZPSMLL3 = ''     /*      LENGTH        */
    &ZPSMLL4 = ''     /*      TTR           */
    &ZPSMLL5 = ''     /*      ATTR          */
    &ZPSMLL6 = ''     /*      AUTH          */
    &ZPSMLL7 = ''     /*      MAIN          */
    &ZPSMLL8 = ''     /*      MODE          */
    &ZPSMLL7 = 1
  ELSE
    &ZCMD = 'SORT MAIN DESCENDING'
    &ZPSMLL7 = ''
IF (&ZCMD = 'MODE')
  IF (&ZPSMLL8 = '')
    &ZCMD = 'SORT MODE ASCENDING'
    &ZPSMLL1 = ''     /*      NAME          */
    &ZPSMLL2 = ''     /*      CREATED       */
    &ZPSMLL3 = ''     /*      LENGTH        */
    &ZPSMLL4 = ''     /*      TTR           */
    &ZPSMLL5 = ''     /*      ATTR          */
    &ZPSMLL6 = ''     /*      AUTH          */
    &ZPSMLL7 = ''     /*      MAIN          */
    &ZPSMLL8 = ''     /*      MODE          */
    &ZPSMLL8 = 1
  ELSE
    &ZCMD = 'SORT MODE DESCENDING'
    &ZPSMLL8 = ''

/* Start NRETRIEV code */
VGET (ZVERB)
.NRET = OFF
IF (&ZVERB = NRETRIEV)
  .NRET = DSN
  IF (&ZNRDS = YES)
    &ZCMD = 'C &ZNRODSN'
    .MSG = ISRDS014
  ELSE .MSG = ISRDS012
/* End NRETRIEV code */

/* Start RefList code */
VGET (ZRDSN ZRVOL) SHARED
IF (&ZRDSN ¬= ' ')
  VGET (ZREFVOLM) PROFILE
  IF (&ZREFVOLM = 'ON')
    &VOL1 = &ZRVOL
  ELSE                                                  /* OW15849*/
    &VOL1 = &Z                                          /* OW15849*/
  IF (&VOL1 = ' ')
    &ZCMD = 'C &ZRDSN'
  ELSE
    &ZCMD = 'C &ZRDSN VOL(&VOL1)'
  &ZDSVOL = &VOL1
  &ZRDSN = ' '
  &ZRVOL = ' '
  VPUT (ZRDSN ZRVOL) SHARED
  VPUT (ZDSVOL) SHARED
  VGET (ZELIST) PROFILE
  IF (&ZELIST ¬= 'EXECUTE') .MSG = ISRDS003
VGET (DSALSEL) SHARED
IF (&DSALSEL ¬= ' ')
  VGET (DSA1,DSA2,DSA6,DSA7) SHARED
  IF (&DSA7 = ' ')
    &ZCMD = 'C ''&DSA1..&DSA2..&DSA6'''
  ELSE
    &ZCMD = 'C ''&DSA1..&DSA2..&DSA6'';ML &DSA7'
  &DSALSEL = ' '
  VPUT (DSALSEL) SHARED
  VGET (ZELIST) PROFILE
  IF (&ZELIST ¬= 'EXECUTE') .MSG = ISRDS003
/* End RefList code */

&PPRE = TRUNC(&ZCMD,3)
IF (&PPRE = 'c ''' OR           /* cHANGE ' command?     */
    &PPRE = 'C ''')             /* Change ' command?     */
  &PDSN = TRUNC(&ZCMD,2)
  &PDSN = .TRAIL
  &PFC = TRUNC(&PDSN,1)         /* If first character    */
  IF (&PFC = '''')              /*   of DSN is "'" check */
    &PREM = .TRAIL              /*   to see if last "'"  */
    &PREM2 = TRUNC(&PREM,'''')  /*   is missing.         */
    IF (&PREM2 = &PREM)         /* If last "'" missing   */
      &ZCMD = '&ZCMD&PFC'       /*   add it to the end.  */

&PPRE = TRUNC(&ZCMD,4)
IF (&PPRE = 'ch ''' OR          /* ch-ANGE ' command?    */
    &PPRE = 'CH ''')            /* CH-ange ' command?    */
  &PDSN = TRUNC(&ZCMD,3)
  &PDSN = .TRAIL
  &PFC = TRUNC(&PDSN,1)         /* If first character    */
  IF (&PFC = '''')              /*   of DSN is "'" check */
    &PREM = .TRAIL              /*   to see if last "'"  */
    &PREM2 = TRUNC(&PREM,'''')  /*   is missing.         */
    IF (&PREM2 = &PREM)         /* If last "'" missing   */
      &ZCMD = '&ZCMD&PFC'       /*   add it to the end.  */

&PPRE = TRUNC(&ZCMD,5)
IF (&PPRE = 'cha ''' OR         /* cha-NGE ' command?    */
    &PPRE = 'CHA ''')           /* CHA-nge ' command?    */
  &PDSN = TRUNC(&ZCMD,4)
  &PDSN = .TRAIL
  &PFC = TRUNC(&PDSN,1)         /* If first character    */
  IF (&PFC = '''')              /*   of DSN is "'" check */
    &PREM = .TRAIL              /*   to see if last "'"  */
    &PREM2 = TRUNC(&PREM,'''')  /*   is missing.         */
    IF (&PREM2 = &PREM)         /* If last "'" missing   */
      &ZCMD = '&ZCMD&PFC'       /*   add it to the end.  */
)PNTS
FIELD(name    ) VAR(ZCMD) VAL('SORT NAME')
FIELD(lenlked ) VAR(ZCMD) VAL('SORT DATE')
FIELD(attr    ) VAR(ZCMD) VAL('SORT ATTR')
FIELD(ac      ) VAR(ZCMD) VAL('SORT AUTH')
FIELD(mode    ) VAR(ZCMD) VAL('SORT MODE')
FIELD(main    ) VAR(ZCMD) VAL('SORT MAIN')
FIELD(length  ) VAR(ZCMD) VAL('SORT LENGTH')
FIELD(ttr     ) VAR(ZCMD) VAL('SORT TTR')
)END
