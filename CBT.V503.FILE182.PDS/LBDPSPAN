    &ZPSMLL5 = ''     /*      ATTR          */
    &ZPSMLL6 = ''     /*      AUTH          */
    &ZPSMLL7 = ''     /*      MAIN          */
    &ZPSMLL8 = ''     /*      MODE          */
    &ZPSMLL7 = 1
  ELSE
    &ZCMD = 'SORT MAIN DESCENDING'
    &ZPSMLL7 = ''
IF (&ZCMD = 'MODE')
  IF (&ZPSMLL8 = '')
    &ZCMD = 'SORT MODE ASCENDING'
    &ZPSMLL1 = ''     /*      NAME          */
    &ZPSMLL2 = ''     /*      CREATED       */
    &ZPSMLL3 = ''     /*      LENGTH        */
    &ZPSMLL4 = ''     /*      TTR           */
    &ZPSMLL5 = ''     /*      ATTR          */
    &ZPSMLL6 = ''     /*      AUTH          */
    &ZPSMLL7 = ''     /*      MAIN          */
    &ZPSMLL8 = ''     /*      MODE          */
    &ZPSMLL8 = 1
  ELSE
    &ZCMD = 'SORT MODE DESCENDING'
    &ZPSMLL8 = ''

/* Start NRETRIEV code */
VGET (ZVERB)
.NRET = OFF
IF (&ZVERB = NRETRIEV)
  .NRET = DSN
  IF (&ZNRDS = YES)
    &ZCMD = 'C &ZNRODSN'
    .MSG = ISRDS014
  ELSE .MSG = ISRDS012
/* End NRETRIEV code */

/* Start RefList code */
VGET (ZRDSN ZRVOL) SHARED
IF (&ZRDSN ¬= ' ')
  VGET (ZREFVOLM) PROFILE
  IF (&ZREFVOLM = 'ON')
    &VOL1 = &ZRVOL
  ELSE                                                  /* OW15849*/
    &VOL1 = &Z                                          /* OW15849*/
  IF (&VOL1 = ' ')
    &ZCMD = 'C &ZRDSN'
  ELSE
    &ZCMD = 'C &ZRDSN VOL(&VOL1)'
  &ZDSVOL = &VOL1
  &ZRDSN = ' '
  &ZRVOL = ' '
  VPUT (ZRDSN ZRVOL) SHARED
  VPUT (ZDSVOL) SHARED
  VGET (ZELIST) PROFILE
  IF (&ZELIST ¬= 'EXECUTE') .MSG = ISRDS003
VGET (DSALSEL) SHARED
IF (&DSALSEL ¬= ' ')
  VGET (DSA1,DSA2,DSA6,DSA7) SHARED
  IF (&DSA7 = ' ')
    &ZCMD = 'C ''&DSA1..&DSA2..&DSA6'''
  ELSE
    &ZCMD = 'C ''&DSA1..&DSA2..&DSA6'';ML &DSA7'
  &DSALSEL = ' '
  VPUT (DSALSEL) SHARED
  VGET (ZELIST) PROFILE
  IF (&ZELIST ¬= 'EXECUTE') .MSG = ISRDS003
/* End RefList code */

&PPRE = TRUNC(&ZCMD,3)
IF (&PPRE = 'c ''' OR           /* cHANGE ' command?     */
    &PPRE = 'C ''')             /* Change ' command?     */
  &PDSN = TRUNC(&ZCMD,2)
  &PDSN = .TRAIL
  &PFC = TRUNC(&PDSN,1)         /* If first character    */
  IF (&PFC = '''')              /*   of DSN is "'" check */
    &PREM = .TRAIL              /*   to see if last "'"  */
    &PREM2 = TRUNC(&PREM,'''')  /*   is missing.         */
    IF (&PREM2 = &PREM)         /* If last "'" missing   */
      &ZCMD = '&ZCMD&PFC'       /*   add it to the end.  */

&PPRE = TRUNC(&ZCMD,4)
IF (&PPRE = 'ch ''' OR          /* ch-ANGE ' command?    */
    &PPRE = 'CH ''')            /* CH-ange ' command?    */
  &PDSN = TRUNC(&ZCMD,3)
  &PDSN = .TRAIL
  &PFC = TRUNC(&PDSN,1)         /* If first character    */
  IF (&PFC = '''')              /*   of DSN is "'" check */
    &PREM = .TRAIL              /*   to see if last "'"  */
    &PREM2 = TRUNC(&PREM,'''')  /*   is missing.         */
    IF (&PREM2 = &PREM)         /* If last "'" missing   */
      &ZCMD = '&ZCMD&PFC'       /*   add it to the end.  */

&PPRE = TRUNC(&ZCMD,5)
IF (&PPRE = 'cha ''' OR         /* cha-NGE ' command?    */
    &PPRE = 'CHA ''')           /* CHA-nge ' command?    */
  &PDSN = TRUNC(&ZCMD,4)
  &PDSN = .TRAIL
  &PFC = TRUNC(&PDSN,1)         /* If first character    */
  IF (&PFC = '''')              /*   of DSN is "'" check */
    &PREM = .TRAIL              /*   to see if last "'"  */
    &PREM2 = TRUNC(&PREM,'''')  /*   is missing.         */
    IF (&PREM2 = &PREM)         /* If last "'" missing   */
      &ZCMD = '&ZCMD&PFC'       /*   add it to the end.  */
)PNTS
FIELD(name    ) VAR(ZCMD) VAL('SORT NAME')
FIELD(ttr     ) VAR(ZCMD) VAL('SORT TTR')
FIELD(main    ) VAR(ZCMD) VAL('SORT MAIN')
FIELD(length  ) VAR(ZCMD) VAL('SORT LENGTH')
)END
./ ADD NAME=P86SET@  0111-97337-16236-0915-00063-00047-00000-T311LBD 49
)ATTR DEFAULT(%+_)
    % TYPE(TEXT) INTENS(HIGH) COLOR(WHITE) SKIP(ON)
    ¢ TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW)
    + TYPE(TEXT) INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    _ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED)
    ¬ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED) PAD(_)
    ¦ TYPE(INPUT) INTENS(HIGH) CAPS(OFF)
    $ type(output) intens(high) caps(off) just(left) pas(on) hilite(uscore)
)BODY EXPAND(\\)
+-\-\-¢PDS Set Default Selection+-\-\-
%OPTION  ===>_ZCMD
+
+
 Choose one of the following (point-and-shoot):
+
    $SETENV+ +- Set default environmental options
    $SETSEL+ +- Set default select alias commands
    $SETLOG+ +- Set default Log limit values
    $SETML+  +- Set default member list options
    $SETV+   +- Set default LISTV options
    $SETLF+  +- Set default LISTFILE/LISTC options
    $SETTRAP++- Set default TRAP options
)INIT
 .HELP = P86SET@P
 &setenv  = 'SETENV'
 &setsel  = 'SETSEL'
 &setlog  = 'SETLOG'
 &setml   = 'SETML'
 &setv    = 'SETV'
 &setlf   = 'SETLF'
 &settrap = 'SETTRAP'
)PROC
 &PDSZSEL=TRANS(TRUNC(&ZCMD,'.')
        SETENV,'SETENV'
         SETEN,'SETENV'
          SETE,'SETENV'
        SETSEL,'SETSEL'
         SETSE,'SETSEL'
          SETS,'SETSEL'
        SETLOG,'SETLOG'
         SETLO,'SETLOG'
          SETL,'SETLOG'
         SETLF,'SETLF'
         SETML,'SETML'
          SETM,'SETML'
          SETV,'SETV'
       SETTRAP,'SETTRAP'
        SETTRA,'SETTRAP'
         SETTR,'SETTRAP'
          SETT,'SETTRAP'
           ' ',' '
             *,'?')
 &ZTRAIL=.TRAIL
 &OPERS=.TRAIL
)PNTS
FIELD(setenv  ) VAR(ZCMD) VAL(SETENV )
FIELD(setsel  ) VAR(ZCMD) VAL(SETSEL )
FIELD(setlog  ) VAR(ZCMD) VAL(SETLOG )
FIELD(setml   ) VAR(ZCMD) VAL(SETML  )
FIELD(setv    ) VAR(ZCMD) VAL(SETV   )
FIELD(setlf   ) VAR(ZCMD) VAL(SETLF  )
FIELD(settrap ) VAR(ZCMD) VAL(SETTRAP)
)END
