.ZVARS=ZCMD
)BODY EXPAND(\\)
!# Memlist# Functions# Options# Special# Defaults# Help+
"-------------------------------------------------------------------------------
+\ \¢Active Catalog Display+\ \
!Command ===>¦ZCMD                                            !Scroll ===>_Z   +
Enter an ISPF command or a special control code:
+  -----------------------------------------------------------------------------
}CMD +DATA/MSG TYPE STATUS  HIT% $num$vol   $dsn                              +
)MODEL
_Z   _Z       @Z   @Z     @Z     @Z  @Z     @Z
)INIT
   &OPT3 = 1
   .HELP  = P86PNO@
   .ZVARS = '(ZSCBR PDSXOPT PDSXDATA PDSXTYPE +
             PDSXSTAT PDSXADDR PDSXNUM PDSXVOL PDSXDSN)'
 &num     = 'NUM'
 &vol     = 'VOLUMES'
 &dsn     = '---------DATA SET NAME ----------'
)PROC
IF (&ZCMD = 'MLALL')
  &ZCMD = 'MEMLIST :'
IF (&ZCMD = 'MLTO')
  &ZCMD = 'MEMLIST : TODAY'
IF (&ZCMD = 'MLYES')
  &ZCMD = 'MEMLIST : YESTERDAY'
IF (&ZCMD = 'MLWE')
  &ZCMD = 'MEMLIST : WEEK'
IF (&ZCMD = 'MLMON')
  &ZCMD = 'MEMLIST : MONTH'
IF (&ZCMD = 'MLQTR')
  &ZCMD = 'MEMLIST : QUARTER'
IF (&ZCMD = 'MLYR')
  &ZCMD = 'MEMLIST : YEAR'
IF (&ZCMD = 'SORTPDC')
  &ZCMD = 'SORT ?'
IF (&ZCMD = 'FINDPDC')
  &ZCMD = 'F ?'
IF (&ZCMD = 'EXCLALL')
  &ZCMD = 'X ALL'
IF (&ZCMD = 'EXCLUP')
  &ZCMD = 'X UP'
IF (&ZCMD = 'EXCLDN')
  &ZCMD = 'X DN'
IF (&ZCMD = 'N1')
  &ZCMD = 1
IF (&ZCMD = 'N2')
  &ZCMD = 2
IF (&ZCMD = 'N3')
  &ZCMD = 3
IF (&ZCMD = 'N4')
  &ZCMD = 4
IF (&ZCMD = 'N5')
  &ZCMD = 5
IF (&ZCMD = 'N6')
  &ZCMD = 6
IF (&ZCMD = 'N7')
  &ZCMD = 7
IF (&ZCMD = 'N8')
  &ZCMD = 8
IF (&ZCMD = 'N9')
  &ZCMD = 9
IF (&ZCMD = 'NUMBER')
  IF (&ZPSCAX1 = '')
    &ZCMD = 'SORT NUMBER ASCENDING'
    &ZPSCAX1 = ''     /*      NUMBER        */
    &ZPSCAX2 = ''     /*      VOLUME        */
    &ZPSCAX3 = ''     /*      DSNAME        */
    &ZPSCAX1 = 1
  ELSE
    &ZCMD = 'SORT NUMBER DESCENDING'
    &ZPSCAX1 = ''
IF (&ZCMD = 'VOLUME')
  IF (&ZPSCAX2 = '')
    &ZCMD = 'SORT VOLUME ASCENDING'
    &ZPSCAX1 = ''     /*      NUMBER        */
    &ZPSCAX2 = ''     /*      VOLUME        */
    &ZPSCAX3 = ''     /*      DSNAME        */
    &ZPSCAX2 = 1
  ELSE
    &ZCMD = 'SORT VOLUME DESCENDING'
    &ZPSCAX2 = ''
IF (&ZCMD = 'DSNAME')
  IF (&ZPSCAX3 = '')
    &ZCMD = 'SORT DSNAME ASCENDING'
    &ZPSCAX1 = ''     /*      NUMBER        */
    &ZPSCAX2 = ''     /*      VOLUME        */
    &ZPSCAX3 = ''     /*      DSNAME        */
    &ZPSCAX3 = 1
  ELSE
    &ZCMD = 'SORT DSNAME DESCENDING'
    &ZPSCAX3 = ''
)PNTS
FIELD(num) VAR(ZCMD) VAL('SORT NUMBER')
FIELD(vol) VAR(ZCMD) VAL('SORT VOLUME')
FIELD(dsn) VAR(ZCMD) VAL('SORT DSNAME')
)END
