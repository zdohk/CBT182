.ZVARS=ZCMD
)BODY EXPAND(\\)
%# Memlist# Functions# Options# Special# Defaults# Help+
"-------------------------------------------------------------------------------
+\ \¢MEMLIST Source Member List 2+\ \
%Command ===>¦ZCMD                                            %Scroll ===>_Z   +
Enter an ISPF command, a PDS subcommand or a special control code:
%&PDSMSG  ----------------------------------------------------------------------
}CMD $name    }DATA    +ALIASOF  $ttr     $vmod    $changed       $size $id
)MODEL
_Z   ¬Z       _Z       @Z        @Z        @Z      @Z       @Z    @Z    @Z
)INIT
   &OPT3 = 1
   .NRET = ON
   .HELP  = P86PN6@
   .ZVARS = '(ZSCBR MEMLOPT  MEMLNAME MEMLDATA MEMLALIS MEMLTTR  +
            MEMLVMOD MEMLMDAT MEMLMTIM MEMLCSIZ MEMLMID)'
 &name   = 'NAME'
 &ttr    = 'TTR'
 &vmod   = 'VER.MOD'
 &changed = 'LAST MODIFIED'
 &size   = ' SIZE'
 &id     = 'ID'
)REINIT
   .NRET = ON
   REFRESH(*)
)PROC
IF (&ZCMD = 'MLALL')
  &ZCMD = 'MEMLIST :'
IF (&ZCMD = 'MLTO')
  &ZCMD = 'MEMLIST : TODAY'
IF (&ZCMD = 'MLYES')
  &ZCMD = 'MEMLIST : YESTERDAY'
IF (&ZCMD = 'MLWE')
  &ZCMD = 'MEMLIST : WEEK'
IF (&ZCMD = 'MLMON')
  &ZCMD = 'MEMLIST : MONTH'
IF (&ZCMD = 'MLQTR')
  &ZCMD = 'MEMLIST : QUARTER'
IF (&ZCMD = 'MLYR')
  &ZCMD = 'MEMLIST : YEAR'
IF (&ZCMD = 'SORTPDC')
  &ZCMD = 'SORT ?'
IF (&ZCMD = 'FINDPDC')
  &ZCMD = 'F ?'
IF (&ZCMD = 'CONTAPDC')
  &ZCMD = 'CONTROL ALL'
IF (&ZCMD = 'USAGAPDC')
  &ZCMD = 'USAGE ALL'
IF (&ZCMD = 'CONTGPDC')
  &ZCMD = 'CONTROL GEO'
IF (&ZCMD = 'EXCLALL')
  &ZCMD = 'X ALL'
IF (&ZCMD = 'EXCLUP')
  &ZCMD = 'X UP'
IF (&ZCMD = 'EXCLDN')
  &ZCMD = 'X DN'
IF (&ZCMD = 'CHGMENU')
  &ZCMD = 'O.UT.CHGMENU'
IF (&ZCMD = 'N1')
  &ZCMD = 1
IF (&ZCMD = 'N2')
  &ZCMD = 2
IF (&ZCMD = 'N3')
  &ZCMD = 3
IF (&ZCMD = 'N4')
  &ZCMD = 4
IF (&ZCMD = 'N5')
  &ZCMD = 5
IF (&ZCMD = 'N6')
  &ZCMD = 6
IF (&ZCMD = 'N7')
  &ZCMD = 7
IF (&ZCMD = 'N8')
  &ZCMD = 8
IF (&ZCMD = 'N9')
  &ZCMD = 9
IF (&ZCMD = 'NAME')
  IF (&ZPSMLS1 = '')
    &ZCMD = 'SORT NAME ASCENDING'
    &ZPSMLS1 = ''     /*      NAME          */
    &ZPSMLS2 = ''     /*      VERSION       */
    &ZPSMLS3 = ''     /*      CREATED       */
    &ZPSMLS4 = ''     /*      MODIFIED      */
    &ZPSMLS5 = ''     /*      SIZE          */
    &ZPSMLS6 = ''     /*      ID            */
    &ZPSMLS7 = ''     /*      TTR           */
    &ZPSMLS1 = 1
  ELSE
    &ZCMD = 'SORT NAME DESCENDING'
    &ZPSMLS1 = ''
IF (&ZCMD = 'TTR')
  IF (&ZPSMLS7 = '')
    &ZCMD = 'SORT TTR ASCENDING'
    &ZPSMLS1 = ''     /*      NAME          */
    &ZPSMLS2 = ''     /*      VERSION       */
    &ZPSMLS3 = ''     /*      CREATED       */
    &ZPSMLS4 = ''     /*      MODIFIED      */
    &ZPSMLS5 = ''     /*      SIZE          */
    &ZPSMLS6 = ''     /*      ID            */
    &ZPSMLS7 = ''     /*      TTR           */
    &ZPSMLS7 = 1
  ELSE
    &ZCMD = 'SORT TTR DESCENDING'
    &ZPSMLS7 = ''
IF (&ZCMD = 'VERSION')
  IF (&ZPSMLS2 = '')
    &ZCMD = 'SORT VERSION DESCENDING'
    &ZPSMLS1 = ''     /*      NAME          */
    &ZPSMLS2 = ''     /*      VERSION       */
    &ZPSMLS3 = ''     /*      CREATED       */
    &ZPSMLS4 = ''     /*      MODIFIED      */
    &ZPSMLS5 = ''     /*      SIZE          */
    &ZPSMLS6 = ''     /*      ID            */
    &ZPSMLS7 = ''     /*      TTR           */
    &ZPSMLS2 = 1
  ELSE
    &ZCMD = 'SORT VERSION ASCENDING'
    &ZPSMLS2 = ''
IF (&ZCMD = 'MODIFIED')
  IF (&ZPSMLS4 = '')
    &ZCMD = 'SORT MODIFIED DESCENDING'
    &ZPSMLS1 = ''     /*      NAME          */
    &ZPSMLS2 = ''     /*      VERSION       */
    &ZPSMLS3 = ''     /*      CREATED       */
    &ZPSMLS4 = ''     /*      MODIFIED      */
    &ZPSMLS5 = ''     /*      SIZE          */
    &ZPSMLS6 = ''     /*      ID            */
    &ZPSMLS7 = ''     /*      TTR           */
    &ZPSMLS4 = 1
  ELSE
    &ZCMD = 'SORT MODIFIED ASCENDING'
    &ZPSMLS4 = ''
IF (&ZCMD = 'SIZE')
  IF (&ZPSMLS5 = '')
    &ZCMD = 'SORT SIZE DESCENDING'
    &ZPSMLS1 = ''     /*      NAME          */
    &ZPSMLS2 = ''     /*      VERSION       */
    &ZPSMLS3 = ''     /*      CREATED       */
    &ZPSMLS4 = ''     /*      MODIFIED      */
    &ZPSMLS5 = ''     /*      SIZE          */
    &ZPSMLS6 = ''     /*      ID            */
    &ZPSMLS7 = ''     /*      TTR           */
    &ZPSMLS5 = 1
  ELSE
    &ZCMD = 'SORT SIZE ASCENDING'
    &ZPSMLS5 = ''
IF (&ZCMD = 'ID')
  IF (&ZPSMLS6 = '')
    &ZCMD = 'SORT ID ASCENDING'
    &ZPSMLS1 = ''     /*      NAME          */
    &ZPSMLS2 = ''     /*      VERSION       */
    &ZPSMLS3 = ''     /*      CREATED       */
    &ZPSMLS4 = ''     /*      MODIFIED      */
    &ZPSMLS5 = ''     /*      SIZE          */
    &ZPSMLS6 = ''     /*      ID            */
    &ZPSMLS7 = ''     /*      TTR           */
    &ZPSMLS6 = 1
  ELSE
    &ZCMD = 'SORT ID DESCENDING'
    &ZPSMLS6 = ''

/* Start NRETRIEV code */
VGET (ZVERB)
.NRET = OFF
IF (&ZVERB = NRETRIEV)
  .NRET = DSN
  IF (&ZNRDS = YES)
    &ZCMD = 'C &ZNRODSN'
    .MSG = ISRDS014
  ELSE .MSG = ISRDS012
/* End NRETRIEV code */

/* Start RefList code */
VGET (ZRDSN ZRVOL) SHARED
IF (&ZRDSN ¬= ' ')
  VGET (ZREFVOLM) PROFILE
  IF (&ZREFVOLM = 'ON')
    &VOL1 = &ZRVOL
  ELSE                                                  /* OW15849*/
    &VOL1 = &Z                                          /* OW15849*/
  IF (&VOL1 = ' ')
    &ZCMD = 'C &ZRDSN'
  ELSE
    &ZCMD = 'C &ZRDSN VOL(&VOL1)'
  &ZDSVOL = &VOL1
  &ZRDSN = ' '
  &ZRVOL = ' '
  VPUT (ZRDSN ZRVOL) SHARED
  VPUT (ZDSVOL) SHARED
  VGET (ZELIST) PROFILE
  IF (&ZELIST ¬= 'EXECUTE') .MSG = ISRDS003
VGET (DSALSEL) SHARED
IF (&DSALSEL ¬= ' ')
  VGET (DSA1,DSA2,DSA6,DSA7) SHARED
  IF (&DSA7 = ' ')
    &ZCMD = 'C ''&DSA1..&DSA2..&DSA6'''
  ELSE
    &ZCMD = 'C ''&DSA1..&DSA2..&DSA6'';ML &DSA7'
  &DSALSEL = ' '
  VPUT (DSALSEL) SHARED
  VGET (ZELIST) PROFILE
  IF (&ZELIST ¬= 'EXECUTE') .MSG = ISRDS003
/* End RefList code */

&PPRE = TRUNC(&ZCMD,3)
IF (&PPRE = 'c ''' OR           /* cHANGE ' command?     */
    &PPRE = 'C ''')             /* Change ' command?     */
  &PDSN = TRUNC(&ZCMD,2)
  &PDSN = .TRAIL
  &PFC = TRUNC(&PDSN,1)         /* If first character    */
  IF (&PFC = '''')              /*   of DSN is "'" check */
    &PREM = .TRAIL              /*   to see if last "'"  */
    &PREM2 = TRUNC(&PREM,'''')  /*   is missing.         */
    IF (&PREM2 = &PREM)         /* If last "'" missing   */
      &ZCMD = '&ZCMD&PFC'       /*   add it to the end.  */

&PPRE = TRUNC(&ZCMD,4)
IF (&PPRE = 'ch ''' OR          /* ch-ANGE ' command?    */
    &PPRE = 'CH ''')            /* CH-ange ' command?    */
  &PDSN = TRUNC(&ZCMD,3)
  &PDSN = .TRAIL
  &PFC = TRUNC(&PDSN,1)         /* If first character    */
  IF (&PFC = '''')              /*   of DSN is "'" check */
    &PREM = .TRAIL              /*   to see if last "'"  */
    &PREM2 = TRUNC(&PREM,'''')  /*   is missing.         */
    IF (&PREM2 = &PREM)         /* If last "'" missing   */
      &ZCMD = '&ZCMD&PFC'       /*   add it to the end.  */

&PPRE = TRUNC(&ZCMD,5)
IF (&PPRE = 'cha ''' OR         /* cha-NGE ' command?    */
    &PPRE = 'CHA ''')           /* CHA-nge ' command?    */
  &PDSN = TRUNC(&ZCMD,4)
  &PDSN = .TRAIL
  &PFC = TRUNC(&PDSN,1)         /* If first character    */
  IF (&PFC = '''')              /*   of DSN is "'" check */
    &PREM = .TRAIL              /*   to see if last "'"  */
    &PREM2 = TRUNC(&PREM,'''')  /*   is missing.         */
    IF (&PREM2 = &PREM)         /* If last "'" missing   */
      &ZCMD = '&ZCMD&PFC'       /*   add it to the end.  */
)PNTS
FIELD(name    ) VAR(ZCMD) VAL('SORT NAME')
FIELD(ttr     ) VAR(ZCMD) VAL('SORT TTR')
FIELD(vmod    ) VAR(ZCMD) VAL('SORT VERSION')
FIELD(changed ) VAR(ZCMD) VAL('SORT MODIFIED')
FIELD(size    ) VAR(ZCMD) VAL('SORT SIZE')
FIELD(id      ) VAR(ZCMD) VAL('SORT ID')
)END
