 ACTION RUN(TUTOR) PARM('P86BIRTH')
)ABCINIT
.ZVARS=ZCMD
)BODY EXPAND(\\)
%# Memlist# Functions# Options# Special# Defaults# Help+
"-------------------------------------------------------------------------------
+\ \¢List Volumes+\ \
%Command ===>¦ZCMD                                            %Scroll ===>_Z   +
Enter an ISPF command or a special control code:
+  -----------------------------------------------------------------------------
}CMD $volume}  DATA  $dev1$dev2 +MOUNT$use$tfree           $large      + STATUS
+----$name  +--------$addr$type  +ATTR$cnt$cyltrke         $cyltrk     +--INDC--
)MODEL
_Z   ¬Z     _Z       @Z   @Z     @Z   @Z  !Z      !Z  @Z   !Z      !Z  @Z
)INIT
   &OPT3 = 1
   .HELP  = P86PNI@
   .ZVARS = '(ZSCBR PDSVOPT  PDSVVOL  PDSVDATA PDSVADDR PDSVTYPE +
            PDSVATTR PDSVCNT PDSVTCYL PDSVTTRK PDSVXNUM +
            PDSVLCYL PDSVLTRK PDSVINDC)'
 &volume  = 'VOLUME'
 &dev1    = 'DEV'
 &dev2    = 'DEV'
 &use     = 'USE'
 &tfree   = '---TOTAL FREE---'
 &large   = '--LARGEST--'
 &name    = ' NAME'
 &addr    = 'ADDR'
 &type    = 'TYPE'
 &cyltrke = '    CYL TRK  EXT'
 &cnt     = 'CNT'
 &cyltrk  = '    CYL TRK'
)PROC
IF (&ZCMD = 'MLALL')
  &ZCMD = 'MEMLIST :'
IF (&ZCMD = 'MLTO')
  &ZCMD = 'MEMLIST : TODAY'
IF (&ZCMD = 'MLYES')
  &ZCMD = 'MEMLIST : YESTERDAY'
IF (&ZCMD = 'MLWE')
  &ZCMD = 'MEMLIST : WEEK'
IF (&ZCMD = 'MLMON')
  &ZCMD = 'MEMLIST : MONTH'
IF (&ZCMD = 'MLQTR')
  &ZCMD = 'MEMLIST : QUARTER'
IF (&ZCMD = 'MLYR')
  &ZCMD = 'MEMLIST : YEAR'
IF (&ZCMD = 'SORTPDC')
  &ZCMD = 'SORT ?'
IF (&ZCMD = 'FINDPDC')
  &ZCMD = 'F ?'
IF (&ZCMD = 'EXCLALL')
  &ZCMD = 'X ALL'
IF (&ZCMD = 'EXCLUP')
  &ZCMD = 'X UP'
IF (&ZCMD = 'EXCLDN')
  &ZCMD = 'X DN'
IF (&ZCMD = 'N1')
  &ZCMD = 1
IF (&ZCMD = 'N2')
  &ZCMD = 2
IF (&ZCMD = 'N3')
  &ZCMD = 3
IF (&ZCMD = 'N4')
  &ZCMD = 4
IF (&ZCMD = 'N5')
  &ZCMD = 5
IF (&ZCMD = 'N6')
  &ZCMD = 6
IF (&ZCMD = 'N7')
  &ZCMD = 7
IF (&ZCMD = 'N8')
  &ZCMD = 8
IF (&ZCMD = 'N9')
  &ZCMD = 9
IF (&ZCMD = 'VOLUME')
  IF (&ZPSLVX1 = '')
    &ZCMD = 'SORT VOLUME ASCENDING'
    &ZPSLVX1 = ''     /*      VOLUME        */
    &ZPSLVX2 = ''     /*      ADDRESS       */
    &ZPSLVX3 = ''     /*      DEVTYPE       */
    &ZPSLVX4 = ''     /*      COUNT         */
    &ZPSLVX5 = ''     /*      TOTAL         */
    &ZPSLVX6 = ''     /*      LARGEST       */
    &ZPSLVX7 = ''     /*      USED          */
    &ZPSLVX1 = 1
  ELSE
    &ZCMD = 'SORT VOLUME DESCENDING'
    &ZPSLVX1 = ''
IF (&ZCMD = 'ADDRESS')
  IF (&ZPSLVX2 = '')
    &ZCMD = 'SORT ADDRESS ASCENDING'
    &ZPSLVX1 = ''     /*      VOLUME        */
    &ZPSLVX2 = ''     /*      ADDRESS       */
    &ZPSLVX3 = ''     /*      DEVTYPE       */
    &ZPSLVX4 = ''     /*      COUNT         */
    &ZPSLVX5 = ''     /*      TOTAL         */
    &ZPSLVX6 = ''     /*      LARGEST       */
    &ZPSLVX7 = ''     /*      USED          */
    &ZPSLVX2 = 1
  ELSE
    &ZCMD = 'SORT ADDRESS DESCENDING'
    &ZPSLVX2 = ''
IF (&ZCMD = 'TYPE')
  IF (&ZPSLVX3 = '')
    &ZCMD = 'SORT TYPE ASCENDING'
    &ZPSLVX1 = ''     /*      VOLUME        */
    &ZPSLVX2 = ''     /*      ADDRESS       */
    &ZPSLVX3 = ''     /*      DEVTYPE       */
    &ZPSLVX4 = ''     /*      COUNT         */
    &ZPSLVX5 = ''     /*      TOTAL         */
    &ZPSLVX6 = ''     /*      LARGEST       */
    &ZPSLVX7 = ''     /*      USED          */
    &ZPSLVX3 = 1
  ELSE
    &ZCMD = 'SORT TYPE DESCENDING'
    &ZPSLVX3 = ''
IF (&ZCMD = 'COUNT')
  IF (&ZPSLVX4 = '')
    &ZCMD = 'SORT COUNT ASCENDING'
    &ZPSLVX1 = ''     /*      VOLUME        */
    &ZPSLVX2 = ''     /*      ADDRESS       */
    &ZPSLVX3 = ''     /*      DEVTYPE       */
    &ZPSLVX4 = ''     /*      COUNT         */
    &ZPSLVX5 = ''     /*      TOTAL         */
    &ZPSLVX6 = ''     /*      LARGEST       */
    &ZPSLVX7 = ''     /*      USED          */
    &ZPSLVX4 = 1
  ELSE
    &ZCMD = 'SORT COUNT DESCENDING'
    &ZPSLVX4 = ''
IF (&ZCMD = 'TOTAL')
  IF (&ZPSLVX5 = '')
    &ZCMD = 'SORT TOTAL ASCENDING'
    &ZPSLVX1 = ''     /*      VOLUME        */
    &ZPSLVX2 = ''     /*      ADDRESS       */
    &ZPSLVX3 = ''     /*      DEVTYPE       */
    &ZPSLVX4 = ''     /*      COUNT         */
    &ZPSLVX5 = ''     /*      TOTAL         */
    &ZPSLVX6 = ''     /*      LARGEST       */
    &ZPSLVX7 = ''     /*      USED          */
    &ZPSLVX5 = 1
  ELSE
    &ZCMD = 'SORT TOTAL DESCENDING'
    &ZPSLVX5 = ''
IF (&ZCMD = 'LARGEST')
  IF (&ZPSLVX6 = '')
    &ZCMD = 'SORT LARGEST ASCENDING'
    &ZPSLVX1 = ''     /*      VOLUME        */
    &ZPSLVX2 = ''     /*      ADDRESS       */
    &ZPSLVX3 = ''     /*      DEVTYPE       */
    &ZPSLVX4 = ''     /*      COUNT         */
    &ZPSLVX5 = ''     /*      TOTAL         */
    &ZPSLVX6 = ''     /*      LARGEST       */
    &ZPSLVX7 = ''     /*      USED          */
    &ZPSLVX6 = 1
  ELSE
    &ZCMD = 'SORT LARGEST DESCENDING'
    &ZPSLVX6 = ''
)PNTS
FIELD(volume ) VAR(ZCMD) VAL('SORT VOLUME')
FIELD(dev1   ) VAR(ZCMD) VAL('SORT ADDRESS')
FIELD(dev2   ) VAR(ZCMD) VAL('SORT TYPE')
FIELD(use    ) VAR(ZCMD) VAL('SORT COUNT')
FIELD(tfree  ) VAR(ZCMD) VAL('SORT TOTAL')
FIELD(large  ) VAR(ZCMD) VAL('SORT LARGEST')
FIELD(name   ) VAR(ZCMD) VAL('SORT VOLUME')
FIELD(addr   ) VAR(ZCMD) VAL('SORT ADDRESS')
FIELD(type   ) VAR(ZCMD) VAL('SORT TYPE')
FIELD(cnt    ) VAR(ZCMD) VAL('SORT COUNT')
FIELD(cyltrke) VAR(ZCMD) VAL('SORT TOTAL')
FIELD(cyltrk ) VAR(ZCMD) VAL('SORT LARGEST')
)END
./ ADD NAME=P86PNIPS 0119-98141-16221-1017-00253-00024-00000-T311LBD 51
)ATTR DEFAULT(%+_)
    # TYPE(AB)    FORMAT(MIX)
    " TYPE(ABSL) GE(ON)
    % TYPE(TEXT) INTENS(HIGH) COLOR(WHITE) SKIP(ON)
    ¢ TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW)
    + TYPE(TEXT) INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    ¬ TYPE(OUTPUT) INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    _ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED)
    ¦ TYPE(INPUT) INTENS(HIGH) CAPS(OFF)
    @ TYPE(OUTPUT) INTENS(LOW)
    } TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW) SKIP(ON) HILITE(USCORE)
    $ type(output) intens(high) caps(off) just(asis) pas(on) hilite(uscore)
