.ZVARS=ZCMD
)BODY EXPAND(\\)
%# Memlist# Functions# Options# Special# Defaults# Help+
"-\-\-
+\ \¢List Allocations+\ \
%Command ===>¦ZCMD\ \                                         %Scroll ===>_Z   +
Enter an ISPF command or a special control code:
+  -----------------------------------------------------------------------------
}CMD +DDNAME  }DATA    +TYPE OP STAT ------DATA SET NAME -----------------------
+                       VOLUME  CREATED     ---- DISP ----   BLKSI LRECL RECFM
)MODEL
+---- -------- -------- ------  --------    ---- ---- ----   ----- ----- ----
_Z   ¬Z       _Z       @Z   @Z @Z   @Z
+                      @Z      @Z       +  @Z   @Z   @Z   + @Z    @Z    @Z   +
)INIT
   &OPT3 = 1
   .HELP  = P86PNF@
   .ZVARS = '(ZSCBR PDSAOPT  PDSADDN  PDSADATA PDSATYPE PDSAOCNT +
            PDSASTAT PDSADSN PDSAVOL PDSAMEM PDSAALLC PDSADISP PDSACDSP +
            PDSABLKS PDSALREC PDSARECF)'
)PROC
IF (&ZCMD = 'MLALL')
  &ZCMD = 'MEMLIST :'
IF (&ZCMD = 'MLTO')
  &ZCMD = 'MEMLIST : TODAY'
IF (&ZCMD = 'MLYES')
  &ZCMD = 'MEMLIST : YESTERDAY'
IF (&ZCMD = 'MLWE')
  &ZCMD = 'MEMLIST : WEEK'
IF (&ZCMD = 'MLMON')
  &ZCMD = 'MEMLIST : MONTH'
IF (&ZCMD = 'MLQTR')
  &ZCMD = 'MEMLIST : QUARTER'
IF (&ZCMD = 'MLYR')
  &ZCMD = 'MEMLIST : YEAR'
IF (&ZCMD = 'FINDPDC')
  &ZCMD = 'F ?'
IF (&ZCMD = 'EXCLALL')
  &ZCMD = 'X ALL'
IF (&ZCMD = 'EXCLUP')
  &ZCMD = 'X UP'
IF (&ZCMD = 'EXCLDN')
  &ZCMD = 'X DN'
IF (&ZCMD = 'N1')
  &ZCMD = 1
IF (&ZCMD = 'N2')
  &ZCMD = 2
IF (&ZCMD = 'N3')
  &ZCMD = 3
IF (&ZCMD = 'N4')
  &ZCMD = 4
IF (&ZCMD = 'N5')
  &ZCMD = 5
IF (&ZCMD = 'N6')
  &ZCMD = 6
IF (&ZCMD = 'N7')
  &ZCMD = 7
IF (&ZCMD = 'N8')
  &ZCMD = 8
IF (&ZCMD = 'N9')
  &ZCMD = 9
)END
