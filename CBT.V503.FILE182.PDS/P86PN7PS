)BODY EXPAND(\\)
%# Memlist# Functions# Options# Special# Defaults# Help+               &PDSVERS
"-------------------------------------------------------------------------------
+\ \¢MEMLIST Load Member List 1+\ \
%Command ===>¦ZCMD                                            %Scroll ===>_Z   +
Enter an ISPF command a PDS subcommand or a special control code:
%&PDSMSG  ----------------------------------------------------------------------
}CMD $NAME    }DATA    +ALIASOF $LEN/LKED+  -- $ATTRIBUTES+  --$AC$MODE+$MAIN+
)MODEL
_Z   ¬Z       _Z       @Z       @Z         @Z   @Z   @Z   @Z    @Z@Z   @Z
)INIT
   &OPT3 = 1
   .NRET = ON
   .HELP  = P86PN7@
   .ZVARS = '(ZSCBR MEMLOPT  MEMLNAME MEMLDATA MEMLALIS MEMLLDAX +
            MEMLATT1 MEMLATT2 MEMLDC   MEMLTEST MEMLAPFX MEMLMODE MEMLMNAM)'
   IF (&ZPDSVERS = 'ON')
     &PDSVERS = &PDSPGMVV
   ELSE
     &PDSVERS = ''
)REINIT
   .NRET = ON
   REFRESH(*)
)PROC
IF (&ZCMD = 'MLALL')
  &ZCMD = 'MEMLIST :'
IF (&ZCMD = 'MLTO')
  &ZCMD = 'MEMLIST : TODAY'
IF (&ZCMD = 'MLYES')
  &ZCMD = 'MEMLIST : YESTERDAY'
IF (&ZCMD = 'MLWE')
  &ZCMD = 'MEMLIST : WEEK'
IF (&ZCMD = 'MLMON')
  &ZCMD = 'MEMLIST : MONTH'
IF (&ZCMD = 'MLQTR')
  &ZCMD = 'MEMLIST : QUARTER'
IF (&ZCMD = 'MLYR')
  &ZCMD = 'MEMLIST : YEAR'
IF (&ZCMD = 'SORTPDC')
  &ZCMD = 'SORT ?'
IF (&ZCMD = 'FINDPDC')
  &ZCMD = 'F ?'
IF (&ZCMD = 'CONTAPDC')
  &ZCMD = 'CONTROL ALL'
IF (&ZCMD = 'USAGAPDC')
  &ZCMD = 'USAGE ALL'
IF (&ZCMD = 'CONTGPDC')
  &ZCMD = 'CONTROL GEO'
IF (&ZCMD = 'EXCLALL')
  &ZCMD = 'X ALL'
IF (&ZCMD = 'EXCLUP')
  &ZCMD = 'X UP'
IF (&ZCMD = 'EXCLDN')
  &ZCMD = 'X DN'
IF (&ZCMD = 'CHGMENU')
  &ZCMD = 'O.UT.CHGMENU'
IF (&ZCMD = 'N1')
  &ZCMD = 1
IF (&ZCMD = 'N2')
  &ZCMD = 2
IF (&ZCMD = 'N3')
  &ZCMD = 3
IF (&ZCMD = 'N4')
  &ZCMD = 4
IF (&ZCMD = 'N5')
  &ZCMD = 5
IF (&ZCMD = 'N6')
  &ZCMD = 6
IF (&ZCMD = 'N7')
  &ZCMD = 7
IF (&ZCMD = 'N8')
  &ZCMD = 8
IF (&ZCMD = 'N9')
  &ZCMD = 9
IF (&ZCMD = 'NAME')
  IF (&ZPSMLL1 = '')
    &ZCMD = 'SORT NAME ASCENDING'
    &ZPSMLL1 = ''     /*      NAME          */
    &ZPSMLL2 = ''     /*      CREATED       */
    &ZPSMLL3 = ''     /*      LENGTH        */
    &ZPSMLL4 = ''     /*      TTR           */
    &ZPSMLL5 = ''     /*      ATTR          */
    &ZPSMLL6 = ''     /*      AUTH          */
    &ZPSMLL7 = ''     /*      MAIN          */
    &ZPSMLL8 = ''     /*      MODE          */
    &ZPSMLL1 = 1
  ELSE
    &ZCMD = 'SORT NAME DESCENDING'
    &ZPSMLL1 = ''
IF (&ZCMD = 'CREATED')
  IF (&ZPSMLL2 = '')
    &ZCMD = 'SORT LENLKED  DESCENDING'
    &ZPSMLL1 = ''     /*      NAME          */
    &ZPSMLL2 = ''     /*      CREATED       */
    &ZPSMLL3 = ''     /*      LENGTH        */
    &ZPSMLL4 = ''     /*      TTR           */
    &ZPSMLL5 = ''     /*      ATTR          */
    &ZPSMLL6 = ''     /*      AUTH          */
    &ZPSMLL7 = ''     /*      MAIN          */
    &ZPSMLL8 = ''     /*      MODE          */
    &ZPSMLL2 = 1
  ELSE
    &ZCMD = 'SORT LENLKED ASCENDING'
    &ZPSMLL2 = ''
IF (&ZCMD = 'LENGTH')
  IF (&ZPSMLL3 = '')
    &ZCMD = 'SORT LENGTH DESCENDING'
    &ZPSMLL1 = ''     /*      NAME          */
    &ZPSMLL2 = ''     /*      CREATED       */
    &ZPSMLL3 = ''     /*      LENGTH        */
    &ZPSMLL4 = ''     /*      TTR           */
    &ZPSMLL5 = ''     /*      ATTR          */
    &ZPSMLL6 = ''     /*      AUTH          */
    &ZPSMLL7 = ''     /*      MAIN          */
    &ZPSMLL8 = ''     /*      MODE          */
    &ZPSMLL3 = 1
  ELSE
    &ZCMD = 'SORT LENGTH ASCENDING'
    &ZPSMLL3 = ''
IF (&ZCMD = 'TTR')
  IF (&ZPSMLL4 = '')
    &ZCMD = 'SORT TTR ASCENDING'
    &ZPSMLL1 = ''     /*      NAME          */
    &ZPSMLL2 = ''     /*      CREATED       */
    &ZPSMLL3 = ''     /*      LENGTH        */
    &ZPSMLL4 = ''     /*      TTR           */
    &ZPSMLL5 = ''     /*      ATTR          */
    &ZPSMLL6 = ''     /*      AUTH          */
    &ZPSMLL7 = ''     /*      MAIN          */
    &ZPSMLL8 = ''     /*      MODE          */
    &ZPSMLL4 = 1
  ELSE
    &ZCMD = 'SORT TTR DESCENDING'
    &ZPSMLL4 = ''
IF (&ZCMD = 'LMATTR')
  IF (&ZPSMLL5 = '')
    &ZCMD = 'SORT ATTR ASCENDING'
    &ZPSMLL1 = ''     /*      NAME          */
    &ZPSMLL2 = ''     /*      CREATED       */
    &ZPSMLL3 = ''     /*      LENGTH        */
    &ZPSMLL4 = ''     /*      TTR           */
    &ZPSMLL5 = ''     /*      ATTR          */
    &ZPSMLL6 = ''     /*      AUTH          */
    &ZPSMLL7 = ''     /*      MAIN          */
    &ZPSMLL8 = ''     /*      MODE          */
    &ZPSMLL5 = 1
  ELSE
    &ZCMD = 'SORT ATTR DESCENDING'
    &ZPSMLL5 = ''
IF (&ZCMD = 'AUTH')
  IF (&ZPSMLL6 = '')
    &ZCMD = 'SORT AUTH ASCENDING'
    &ZPSMLL1 = ''     /*      NAME          */
    &ZPSMLL2 = ''     /*      CREATED       */
    &ZPSMLL3 = ''     /*      LENGTH        */
    &ZPSMLL4 = ''     /*      TTR           */
    &ZPSMLL5 = ''     /*      ATTR          */
    &ZPSMLL6 = ''     /*      AUTH          */
    &ZPSMLL7 = ''     /*      MAIN          */
    &ZPSMLL8 = ''     /*      MODE          */
    &ZPSMLL6 = 1
  ELSE
    &ZCMD = 'SORT AUTH DESCENDING'
    &ZPSMLL6 = ''
IF (&ZCMD = 'MAIN')
  IF (&ZPSMLL7 = '')
    &ZCMD = 'SORT MAIN ASCENDING'
    &ZPSMLL1 = ''     /*      NAME          */
    &ZPSMLL2 = ''     /*      CREATED       */
    &ZPSMLL3 = ''     /*      LENGTH        */
    &ZPSMLL4 = ''     /*      TTR           */
    &ZPSMLL5 = ''     /*      ATTR          */
    &ZPSMLL6 = ''     /*      AUTH          */
    &ZPSMLL7 = ''     /*      MAIN          */
    &ZPSMLL8 = ''     /*      MODE          */
    &ZPSMLL7 = 1
  ELSE
    &ZCMD = 'SORT MAIN DESCENDING'
    &ZPSMLL7 = ''
IF (&ZCMD = 'MODE')
  IF (&ZPSMLL8 = '')
    &ZCMD = 'SORT MODE ASCENDING'
    &ZPSMLL1 = ''     /*      NAME          */
    &ZPSMLL2 = ''     /*      CREATED       */
    &ZPSMLL3 = ''     /*      LENGTH        */
    &ZPSMLL4 = ''     /*      TTR           */
    &ZPSMLL5 = ''     /*      ATTR          */
    &ZPSMLL6 = ''     /*      AUTH          */
    &ZPSMLL7 = ''     /*      MAIN          */
    &ZPSMLL8 = ''     /*      MODE          */
    &ZPSMLL8 = 1
  ELSE
    &ZCMD = 'SORT MODE DESCENDING'
    &ZPSMLL8 = ''

/* Start NRETRIEV code */
VGET (ZVERB)
.NRET = OFF
IF (&ZVERB = NRETRIEV)
  .NRET = DSN
  IF (&ZNRDS = YES)
    &ZCMD = 'C &ZNRODSN'
    .MSG = ISRDS014
  ELSE .MSG = ISRDS012
/* End NRETRIEV code */

/* Start RefList code */
VGET (ZRDSN ZRVOL) SHARED
IF (&ZRDSN ¬= ' ')
  VGET (ZREFVOLM) PROFILE
  IF (&ZREFVOLM = 'ON')
    &VOL1 = &ZRVOL
  ELSE                                                  /* OW15849*/
    &VOL1 = &Z                                          /* OW15849*/
  IF (&VOL1 = ' ')
    &ZCMD = 'C &ZRDSN'
  ELSE
    &ZCMD = 'C &ZRDSN VOL(&VOL1)'
  &ZDSVOL = &VOL1
  &ZRDSN = ' '
  &ZRVOL = ' '
  VPUT (ZRDSN ZRVOL) SHARED
  VPUT (ZDSVOL) SHARED
  VGET (ZELIST) PROFILE
  IF (&ZELIST ¬= 'EXECUTE') .MSG = ISRDS003
VGET (DSALSEL) SHARED
IF (&DSALSEL ¬= ' ')
  VGET (DSA1,DSA2,DSA6,DSA7) SHARED
  IF (&DSA7 = ' ')
    &ZCMD = 'C ''&DSA1..&DSA2..&DSA6'''
  ELSE
    &ZCMD = 'C ''&DSA1..&DSA2..&DSA6'';ML &DSA7'
  &DSALSEL = ' '
  VPUT (DSALSEL) SHARED
  VGET (ZELIST) PROFILE
  IF (&ZELIST ¬= 'EXECUTE') .MSG = ISRDS003
/* End RefList code */

*Rexx(* PDS2DSN)
  select
    when abbrev("CHANGE",word(translate(zcmd),1),1) = 1 then do
      parse var zcmd cmd chgdsn restof
      if left(chgdsn,1) = "'" & right(chgdsn,1) <> "'"  then
        zcmd = cmd chgdsn"'" restof
      end
    when abbrev("COPY",word(translate(zcmd),1),3) = 1 then do
      parse var zcmd cmd grp pds2dsn .
      if left(pds2dsn,1) = "'" & right(pds2dsn,1) <> "'"  then
        pds2dsn = pds2dsn"'"
      end
    otherwise
      nop
  end
*EndRexx
VPUT (PDS2DSN)

)PNTS
FIELD(ZPS00001) VAR(ZCMD) VAL(NAME)
FIELD(ZPS00002) VAR(ZCMD) VAL(CREATED)
FIELD(ZPS00003) VAR(ZCMD) VAL(LMATTR)
FIELD(ZPS00004) VAR(ZCMD) VAL(AUTH)
FIELD(ZPS00005) VAR(ZCMD) VAL(MODE)
FIELD(ZPS00006) VAR(ZCMD) VAL(MAIN)
)END
