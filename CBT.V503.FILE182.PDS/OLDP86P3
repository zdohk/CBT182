    &ZPSMLS6 = ''     /*      ID            */
    &ZPSMLS7 = ''     /*      TTR           */
    &ZPSMLS2 = 1
  ELSE
    &ZCMD = 'SORT VERSION ASCENDING'
    &ZPSMLS2 = ''
IF (&ZCMD = 'MODIFIED')
  IF (&ZPSMLS4 = '')
    &ZCMD = 'SORT MODIFIED DESCENDING'
    &ZPSMLS1 = ''     /*      NAME          */
    &ZPSMLS2 = ''     /*      VERSION       */
    &ZPSMLS3 = ''     /*      CREATED       */
    &ZPSMLS4 = ''     /*      MODIFIED      */
    &ZPSMLS5 = ''     /*      SIZE          */
    &ZPSMLS6 = ''     /*      ID            */
    &ZPSMLS7 = ''     /*      TTR           */
    &ZPSMLS4 = 1
  ELSE
    &ZCMD = 'SORT MODIFIED ASCENDING'
    &ZPSMLS4 = ''
IF (&ZCMD = 'SIZE')
  IF (&ZPSMLS5 = '')
    &ZCMD = 'SORT SIZE DESCENDING'
    &ZPSMLS1 = ''     /*      NAME          */
    &ZPSMLS2 = ''     /*      VERSION       */
    &ZPSMLS3 = ''     /*      CREATED       */
    &ZPSMLS4 = ''     /*      MODIFIED      */
    &ZPSMLS5 = ''     /*      SIZE          */
    &ZPSMLS6 = ''     /*      ID            */
    &ZPSMLS7 = ''     /*      TTR           */
    &ZPSMLS5 = 1
  ELSE
    &ZCMD = 'SORT SIZE ASCENDING'
    &ZPSMLS5 = ''
IF (&ZCMD = 'ID')
  IF (&ZPSMLS6 = '')
    &ZCMD = 'SORT ID ASCENDING'
    &ZPSMLS1 = ''     /*      NAME          */
    &ZPSMLS2 = ''     /*      VERSION       */
    &ZPSMLS3 = ''     /*      CREATED       */
    &ZPSMLS4 = ''     /*      MODIFIED      */
    &ZPSMLS5 = ''     /*      SIZE          */
    &ZPSMLS6 = ''     /*      ID            */
    &ZPSMLS7 = ''     /*      TTR           */
    &ZPSMLS6 = 1
  ELSE
    &ZCMD = 'SORT ID DESCENDING'
    &ZPSMLS6 = ''

/* Start NRETRIEV code */
/* End NRETRIEV code */

/* Start RefList code */
VGET (ZRDSN ZRVOL) SHARED
IF (&ZRDSN ¬= ' ')
  VGET (ZREFVOLM) PROFILE
  IF (&ZREFVOLM = 'ON')
    &VOL1 = &ZRVOL
  ELSE                                                  /* OW15849*/
    &VOL1 = &Z                                          /* OW15849*/
  IF (&VOL1 = ' ')
    &ZCMD = 'C &ZRDSN'
  ELSE
    &ZCMD = 'C &ZRDSN VOL(&VOL1)'
  &ZDSVOL = &VOL1
  &ZRDSN = ' '
  &ZRVOL = ' '
  VPUT (ZRDSN ZRVOL) SHARED
  VPUT (ZDSVOL) SHARED
  VGET (ZELIST) PROFILE
  IF (&ZELIST ¬= 'EXECUTE') .MSG = ISRDS003
VGET (DSALSEL) SHARED
IF (&DSALSEL ¬= ' ')
  VGET (DSA1,DSA2,DSA6,DSA7) SHARED
  IF (&DSA7 = ' ')
    &ZCMD = 'C ''&DSA1..&DSA2..&DSA6'''
  ELSE
    &ZCMD = 'C ''&DSA1..&DSA2..&DSA6'';ML &DSA7'
  &DSALSEL = ' '
  VPUT (DSALSEL) SHARED
  VGET (ZELIST) PROFILE
  IF (&ZELIST ¬= 'EXECUTE') .MSG = ISRDS003
/* End RefList code */

&PPRE = TRUNC(&ZCMD,3)
IF (&PPRE = 'c ''' OR           /* cHANGE ' command?     */
    &PPRE = 'C ''')             /* Change ' command?     */
  &PDSN = TRUNC(&ZCMD,2)
  &PDSN = .TRAIL
  &PFC = TRUNC(&PDSN,1)         /* If first character    */
  IF (&PFC = '''')              /*   of DSN is "'" check */
    &PREM = .TRAIL              /*   to see if last "'"  */
    &PREM2 = TRUNC(&PREM,'''')  /*   is missing.         */
    IF (&PREM2 = &PREM)         /* If last "'" missing   */
      &ZCMD = '&ZCMD&PFC'       /*   add it to the end.  */
)PNTS
FIELD(ZPS00001) VAR(ZCMD) VAL(NAME)
FIELD(ZPS00002) VAR(ZCMD) VAL(TTR)
FIELD(ZPS00003) VAR(ZCMD) VAL(VERSION)
FIELD(ZPS00004) VAR(ZCMD) VAL(MODIFIED)
FIELD(ZPS00005) VAR(ZCMD) VAL(SIZE)
FIELD(ZPS00006) VAR(ZCMD) VAL(ID)
)END
