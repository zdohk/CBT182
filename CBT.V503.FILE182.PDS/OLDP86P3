+-\-\-¢List Volumes+-\-\-
%COMMAND ===>¦ZCMD                                            %SCROLL ===>_Z   +
Enter an ISPF command or a special control code:
 %LA+Lista    %7+Output table  %9+Swap panels  %O+Options
 %ML+Memlist  %8+View log      %F+Find         %S+Sort     %X+Above/Below/All
+  -----------------------------------------------------------------------------
}CMD $VOLUME}  DATA  $DEV $DEV  +MOUNT$USE$-- TOTAL FREE --$  LARGEST  + STATUS
+----$ NAME +--------$ADDR$TYPE  +ATTR$CNT$ CYLS  TRKS  NUM$ CYLS  TRKS+--INDC--
)MODEL
_Z   ¬Z     _Z       @Z   @Z     @Z   @Z  !Z    !Z    @Z   !Z    !Z    @Z
)INIT
   .HELP  = P86PNI@
   .ZVARS = '(ZSCBR PDSVOPT  PDSVVOL  PDSVDATA PDSVADDR PDSVTYPE +
            PDSVATTR PDSVCNT PDSVTCYL PDSVTTRK PDSVXNUM +
            PDSVLCYL PDSVLTRK PDSVINDC)'
)PROC
IF (&ZCMD = 'VOLUME')
  IF (&ZPSLVX1 = '')
    &ZCMD = 'SORT VOLUME ASCENDING'
    &ZPSLVX1 = ''     /*      VOLUME        */
    &ZPSLVX2 = ''     /*      ADDRESS       */
    &ZPSLVX3 = ''     /*      DEVTYPE       */
    &ZPSLVX4 = ''     /*      COUNT         */
    &ZPSLVX5 = ''     /*      TOTAL         */
    &ZPSLVX6 = ''     /*      LARGEST       */
    &ZPSLVX7 = ''     /*      USED          */
    &ZPSLVX1 = 1
  ELSE
    &ZCMD = 'SORT VOLUME DESCENDING'
    &ZPSLVX1 = ''
IF (&ZCMD = 'ADDRESS')
  IF (&ZPSLVX2 = '')
    &ZCMD = 'SORT ADDRESS ASCENDING'
    &ZPSLVX1 = ''     /*      VOLUME        */
    &ZPSLVX2 = ''     /*      ADDRESS       */
    &ZPSLVX3 = ''     /*      DEVTYPE       */
    &ZPSLVX4 = ''     /*      COUNT         */
    &ZPSLVX5 = ''     /*      TOTAL         */
    &ZPSLVX6 = ''     /*      LARGEST       */
    &ZPSLVX7 = ''     /*      USED          */
    &ZPSLVX2 = 1
  ELSE
    &ZCMD = 'SORT ADDRESS DESCENDING'
    &ZPSLVX2 = ''
IF (&ZCMD = 'DEVTYPE')
  IF (&ZPSLVX3 = '')
    &ZCMD = 'SORT DEVTYPE ASCENDING'
    &ZPSLVX1 = ''     /*      VOLUME        */
    &ZPSLVX2 = ''     /*      ADDRESS       */
    &ZPSLVX3 = ''     /*      DEVTYPE       */
    &ZPSLVX4 = ''     /*      COUNT         */
    &ZPSLVX5 = ''     /*      TOTAL         */
    &ZPSLVX6 = ''     /*      LARGEST       */
    &ZPSLVX7 = ''     /*      USED          */
    &ZPSLVX3 = 1
  ELSE
    &ZCMD = 'SORT DEVTYPE DESCENDING'
    &ZPSLVX3 = ''
IF (&ZCMD = 'COUNT')
  IF (&ZPSLVX4 = '')
    &ZCMD = 'SORT COUNT ASCENDING'
    &ZPSLVX1 = ''     /*      VOLUME        */
    &ZPSLVX2 = ''     /*      ADDRESS       */
    &ZPSLVX3 = ''     /*      DEVTYPE       */
    &ZPSLVX4 = ''     /*      COUNT         */
    &ZPSLVX5 = ''     /*      TOTAL         */
    &ZPSLVX6 = ''     /*      LARGEST       */
    &ZPSLVX7 = ''     /*      USED          */
    &ZPSLVX4 = 1
  ELSE
    &ZCMD = 'SORT COUNT DESCENDING'
    &ZPSLVX4 = ''
IF (&ZCMD = 'TOTAL')
  IF (&ZPSLVX5 = '')
    &ZCMD = 'SORT TOTAL ASCENDING'
    &ZPSLVX1 = ''     /*      VOLUME        */
    &ZPSLVX2 = ''     /*      ADDRESS       */
    &ZPSLVX3 = ''     /*      DEVTYPE       */
    &ZPSLVX4 = ''     /*      COUNT         */
    &ZPSLVX5 = ''     /*      TOTAL         */
    &ZPSLVX6 = ''     /*      LARGEST       */
    &ZPSLVX7 = ''     /*      USED          */
    &ZPSLVX5 = 1
  ELSE
    &ZCMD = 'SORT TOTAL DESCENDING'
    &ZPSLVX5 = ''
IF (&ZCMD = 'LARGEST')
  IF (&ZPSLVX6 = '')
    &ZCMD = 'SORT LARGEST ASCENDING'
    &ZPSLVX1 = ''     /*      VOLUME        */
    &ZPSLVX2 = ''     /*      ADDRESS       */
    &ZPSLVX3 = ''     /*      DEVTYPE       */
    &ZPSLVX4 = ''     /*      COUNT         */
    &ZPSLVX5 = ''     /*      TOTAL         */
    &ZPSLVX6 = ''     /*      LARGEST       */
    &ZPSLVX7 = ''     /*      USED          */
    &ZPSLVX6 = 1
  ELSE
    &ZCMD = 'SORT LARGEST DESCENDING'
    &ZPSLVX6 = ''
)PNTS
