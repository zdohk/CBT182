//*
//*  THIS MEMBER GENERATES AND EXECUTES THE HELPLIST PROGRAM FOR PDS
//*
//HELPGEN PROC MEMBER=MISSING,
//             INSTLIB='PDS.INSTALL.LIBRARY'        <<=== INSTALL LIB
//* INVOKE HIGH LEVEL ASSEMBLER
//ASM      EXEC  PGM=ASMA90,REGION=32M,
//             PARM=(NORLD,TERM,OBJ,
//             'XREF(SHORT),SIZE(MAX,ABOVE)')
//SYSUT1   DD  UNIT=VIO,SPACE=(4096,(120,120),,,ROUND),DCB=BUFNO=1
//SYSLIN   DD  UNIT=VIO,SPACE=(TRK,(5,5)),DISP=(MOD,PASS),DSN=&&X,
//             DCB=BLKSIZE=3120
//SYSPRINT DD  SYSOUT=*
//SYSTERM  DD  SYSOUT=*
//SYSIN    DD  DISP=SHARE,DSN=&INSTLIB.(&MEMBER)
//SYSLIB   DD  DSN=SYS1.MODGEN,DISP=SHARE
//         DD  DSN=SYS1.MACLIB,DISP=SHARE
//         DD  DSN=&INSTLIB,DISP=SHARE
//       PEND
//*
//*
//HELPLIST EXEC HELPGEN,MEMBER=HELPLIST
//*
//LK1    EXEC  PGM=IEWL,PARM='MAP,LET,NCAL',COND=(0,LT)
//SYSPRINT DD  SYSOUT=*
//IN       DD  DSN=&&X,DISP=(OLD,DELETE)
//SYSLMOD  DD  DISP=SHR,DSN=LINKLIST.LIB            <<=== LINK TARGET
//SYSUT1   DD  UNIT=VIO,SPACE=(TRK,(5,5))
//SYSLIN   DD  *
  INCLUDE  IN
  NAME     HELPLIST(R)                              <<=== LOADMOD NAME
//*
//ONE   EXEC  PGM=HELPLIST                          <<=== LOADMOD NAME
//SYSPRINT DD  SYSOUT=*
//SYSUT1   DD  DSN=SYS2.HELP,DISP=SHR               <<=== HELP LIBRARY
//SYSIN    DD  *
PDS86                                               <<=== HELP MEMBER
