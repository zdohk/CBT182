./ ADD NAME=README
This is not a panel but a short doc describing this member.

Sam Golob has taken the NRETRIEV logic out of these 12 panels so that
they will work with earlier versions of ISPF (before 4.5).

The panels are at the 8.5.25 maintenance level.

./ ADD NAME=P86@CUA  0118-00080-03344-1406-00236-00223-00000-SBGOLOB
)ATTR DEFAULT(%+_)
    # TYPE(AB)    FORMAT(MIX)
    % TYPE(TEXT)  INTENS(HIGH) COLOR(WHITE) SKIP(ON)
    ¢ TYPE(TEXT)  INTENS(HIGH) COLOR(YELLOW)
    + TYPE(TEXT)  INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    _ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED)
    ¬ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED) PAD(_)
    ¦ TYPE(INPUT) INTENS(HIGH) CAPS(OFF)
    " TYPE(ABSL) GE(ON)
)ABC DESC('RefList') MNEM(1)
PDC DESC('Current Data Set List &ZDSCURT.') MNEM(1) ACC(CTRL+ALT+P)
 ACTION RUN(ISRRLIST) PARM('PL1')
PDC DESC('Current Library List &ZDSCURLT.') MNEM(2) ACC(CTRL+SHIFT+P)
 ACTION RUN(ISRRLIST) PARM('LL1')
PDC DESC('List of Personal Data Set Lists') MNEM(1) PDSEP(ON) ACC(CTRL+ALT+O)
 ACTION RUN(ISRRLIST) PARM('PL2')
PDC DESC('List of Personal Library Lists') MNEM(2) ACC(CTRL+SHIFT+O)
 ACTION RUN(ISRRLIST) PARM('LL2')
)ABCINIT
.ZVARS=REFLIST
      VGET (ZCURTB ZCURLTB) PROFILE
      IF (&ZCURTB = &Z) &ZDSCURT = &Z
      ELSE &ZDSCURT= '(&ZCURTB)'
      IF (&ZCURLTB = &Z) &ZDSCURLT = &Z
      ELSE &ZDSCURLT= '(&ZCURLTB)'
)ABC DESC('RefMode') MNEM(2)
PDC DESC('List Execute') UNAVAIL(ZRM1) MNEM(1) ACTION RUN(ISRRLIST) PARM('BEX')
PDC DESC('List Retrieve') UNAVAIL(ZRM2) MNEM(2) ACTION RUN(ISRRLIST) PARM('BRT')
)ABCINIT
.ZVARS=REFMODE
  VGET (ZBLIST) PROFILE
  IF (&ZBLIST = 'EXECUTE')
    &zrm1 = 1
    &zrm2 = 0
    &refmode = 2
  ELSE
    &zrm1 = 0
    &zrm2 = 1
    &refmode = 1
)ABC DESC('Options') MNEM(1)
PDC DESC('PDS Function Selection...') MNEM(5)
 ACTION RUN(F)
PDC DESC('PDS Settings...') MNEM(5)
 ACTION RUN(SET)
PDC DESC('TSO Line Mode') MNEM(5)
 ACTION RUN(L)
PDC DESC('ISPF Dialog Mode') MNEM(5)
 ACTION RUN(I)
PDC DESC('Point-and-Shoot...') MNEM(1)
 ACTION RUN(PSCOLOR)
)ABCINIT
.ZVARS=PDSOPT
)ABC DESC('Help') MNEM(1)
PDC PDCTEXT('Changes to PDS Version 8.5')
 ACTION RUN(TUTOR) PARM('P86CHGS')
PDC PDCTEXT('PDS command tutorial')
 ACTION RUN(TUTOR) PARM('P86PN@PH')
PDC PDCTEXT('"Learning PDS" by Sam Golob')
 ACTION RUN(TUTOR) PARM('P86ART0')
PDC PDCTEXT('About...')
 ACTION RUN(TUTOR) PARM('P86LOGO')
PDC PDCTEXT('Undocumented Items')
 ACTION RUN(TUTOR) PARM('P86UNDOC')
PDC PDCTEXT('StarTool')
 ACTION RUN(TUTOR) PARM('P86STAR')
PDC PDCTEXT('PDSE Libraries')
 ACTION RUN(TUTOR) PARM('P86PDSE')
)ABCINIT
.ZVARS=HELP
)BODY EXPAND(\\)
%# RefList# RefMode# Options# Help+
"-------------------------------------------------------------------------------
+\ \¢PDS Version 8.5+\ \
%Option ===>_ZCMD                                                              +
+
%    I+- Enter ISPMODE
%    M+- Enter MEMLIST with the identified MEMBERS
%    L+- Enter Line mode
%  SET+- Set default options prompt
%blank+- Enter MEMLIST if any member data is entered; ISPMODE otherwise
+
%ISPF Library:
+   Project%===>_PRJ1    %
+   Library%===>_LIB1    %
+   Type   %===>_TYP1    %
+   Members%===>_MEMS             +(set to * or a member group to use MEMLIST)
+
%Other Partitioned or Sequential Data Set or FILE(ddname):
+   Data Set Name %===>_DSN1                                                   %
+   Volume Serial %===>_VOL1  +  (If not cataloged)
+   Volume Set    %===>_VOLSET+  (For a default volume name)
+   MEMLIST Prompt%===>_MEMP+    (yes/no for a MEMLIST prompt panel)
+  ¢PDS+PGM Name  %===>_PGM2    +(latest: PDS86)
)INIT
  .CURSOR = DSN1          /* CSC MOD TO SET CURSOR */
  IF (&PDSPDSN = &Z)
    .CURSOR = MEMS
  IF (&PDSPDSN ¬= &Z)
    .CURSOR = DSN1
  .HELP   = P86PN@PH
  &ZHTOP  = P86PN@PO
  &MEM = &Z
  &MEMS=&Z
  &DSN1 = &PDSPDSN
  &VOL1 = &PDSPVOL
  &VOLSET = &PDSPVST
  &MEMP = &PDSMEMP
  &PGM2 = &PDSPPGM
  IF (&MEMP = &Z)  &MEMP = 'NO'
  IF (&PGM2 = &Z)  &PGM2 = 'PDS86'
  IF (&MEMS = ':') &MEMS = '*'
)REINIT
  .CURSOR = DSN1          /* CSC MOD TO SET CURSOR */
  IF (&DSN1 ¬= &Z)
    &DSN1 = &PDSPDSN
    &VOL1 = &PDSPVOL
    &MEMS = &Z
  REFRESH (*)
)PROC
  /* Start NRETRIEV code */
  /* End NRETRIEV code */
  /* Start RefList code */
  VGET (ZRDSN ZRVOL) SHARED
  IF (&ZRDSN ¬= ' ')
    &DSN1 = &ZRDSN
    VGET (ZREFVOLM) PROFILE
    IF (&ZREFVOLM = 'ON')
      &VOL1 = &ZRVOL
    ELSE                                                  /* OW15849*/
      &VOL1 = &Z                                          /* OW15849*/
    &ZDSVOL = &VOL1
    .CURSOR = DSN1
    &ZRDSN = ' '
    &ZRVOL = ' '
    VPUT (ZRDSN ZRVOL) SHARED
    VPUT (ZDSVOL) SHARED
    VGET (ZBLIST) PROFILE
    IF (&ZBLIST ¬= 'EXECUTE') .MSG = ISRDS003
  VGET (DSALSEL) SHARED
  IF (&DSALSEL ¬= ' ')
    VGET (DSA1,DSA2,DSA6,DSA7) SHARED
    &PRJ1 = &DSA1
    &LIB1 = &DSA2
    &TYP1 = &DSA6
    &MEMS  = &DSA7
    &DSN1 = ' '
    &VOL1 = ' '
    &DSALSEL = ' '
    VPUT (DSALSEL) SHARED
    .CURSOR = MEMS
    VGET (ZBLIST) PROFILE
    IF (&ZBLIST ¬= 'EXECUTE') .MSG = ISRDS003
  /* End RefList code */
  &TYPE = TRUNC (&ZCMD,1)
  &PDSPDSN = &DSN1
  &PDSPVOL = &VOL1
  &PDSPVST = &VOLSET
  &MEMP = TRANS(TRUNC(&MEMP,1),Y,YES,N,NO,*,*)
  VER (&MEMP,NB,LIST,YES,NO)
  &PDSMEMP = &MEMP
  &PDSPPGM = &PGM2
  VPUT PDSMEMP PROFILE
  VPUT (PRJ1 LIB1 TYP1) PROFILE
  VPUT (PDSPDSN PDSPVOL PDSPVST PDSMEMP PDSPPGM) SHARED
  IF (&ZCMD = &Z)  &TYPE = D
  VER (&TYPE,LIST,D,F,I,M,L,S,MSG=PDSM861S)
  IF (&DSN1 = &Z)
    VER (&PRJ1,NB,DSNAME,MSG=PDSM862S)
    VER (&LIB1,NB,DSNAME,MSG=PDSM863S)
    VER (&TYP1,NB,DSNAME,MSG=PDSM864S)
    &OPT2 = 1
    IF (&VOL1 ¬= &Z) .MSG =PDSM867S
    IF (&VOLSET ¬=&Z)  .MSG =PDSM867S
  IF (&DSN1 ¬= &Z)
     &ENT1 = TRUNC(&DSN1,'(')
     &MEM1 = &Z
     IF (&ENT1 ¬= &DSN1) /* MEMBER NAME GIVEN */
        &MEM1 = .TRAIL
        &MEM1 = TRUNC(&MEM1,')')
        &T = TRUNC(&ENT1,1)
        IF (&T = '''')
           &ENT1 = .TRAIL
        IF (&T ¬= '''')
           &T = &ENT1
           IF (&ZPREFIX ¬= &Z)
              &ENT1 = '&ZPREFIX..&ENT1'
     IF (&T = 'FILE', 'FIL' ,'FI' ,'F')
        &T ='FILE'
     IF (&MEM1 = &Z) /* NO MEMBER NAME GIVEN */
        &T = TRUNC(&DSN1,1)
        IF (&T = '''')
           &ENT1 = .TRAIL
           &ENT1 = TRUNC(&ENT1,'''')
        IF (&T ¬= '''')
           &ENT1 = '&ZPREFIX..&DSN1'
           IF (&ZPREFIX = &Z) &ENT1=&DSN1
     IF (&MEM1 ¬= &Z)
        IF (&T ¬= 'FILE')
           &MEMS = &MEM1
     IF (&T ¬= 'FILE')
        &DSN1='''&ENT1'''
        VER (&DSN1,DSNAME,MSG=PDSM869S)
     IF (&VOL1 = &Z)
        &OPT2 = 2
        IF (&VOLSET ¬= &Z)
           &OPT2 = 3
     IF (&VOL1 ¬= &Z)
        &OPT2 = 4
        IF (&VOLSET ¬= &Z)
           &OPT2 = 5
  IF (&MEMS = '*')  &MEMS = ':'
  IF (&TYPE = D)
     IF (&MEMS = &Z) &TYPE = I
        IF (&MEMP=YES)
           &ADDED=' ISPXEQ ML'
  IF (&TYPE = D)
     IF (&MEMS ¬= &Z) &TYPE = M
  IF (&TYPE = I)  &ADDED = ' ISPMODE'
        IF (&MEMP=YES)
           &ADDED=' ISPXEQ ML'
  IF (&TYPE = M)
     &ADDED = ' MEMLIST &MEMS'
     IF (&MEMP = YES)
           &ADDED=' ISPXEQ ML'
  IF (&TYPE = L)  &ADDED = &Z
  IF (&TYPE = S)  &ADDED =' ISPXEQ SETDEF'
  IF (&TYPE = F)  &ADDED =' ISPXEQ O.O.1'  /* panel P86FNFN */
  &ZSEL = TRANS(&OPT2,
                1,'PGM(&PGM2) PARM(&PGM2 ''&PRJ1..&LIB1..&TYP1'' &ADDED)'
                2,'PGM(&PGM2) PARM(&PGM2 &DSN1 &ADDED)'
                3,'PGM(&PGM2) PARM(&PGM2 &DSN1 VOLSET(&VOLSET) &ADDED)'
                4,'PGM(&PGM2) PARM(&PGM2 &DSN1 VOL(&VOL1) &ADDED)'
                5,'PGM(&PGM2) PARM(&PGM2 &DSN1 VOL(&VOL1) VOLS(&VOLSET) &ADDED)'
               ' ',' '
                *,'?' )
)END
./ ADD NAME=P86PNA   0115-99320-03344-1406-00072-00024-00000-SBGOLOB
)ATTR DEFAULT(%¬_)
    % TYPE(TEXT) INTENS(HIGH) COLOR(WHITE) SKIP(ON)
    ¢ TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW)
    ¬ TYPE(TEXT) INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    _ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED)
    ¦ TYPE(INPUT) INTENS(HIGH) CAPS(OFF)
    @ TYPE(OUTPUT) INTENS(LOW)
    # TYPE(OUTPUT) INTENS(HIGH) COLOR(YELLOW)
)BODY EXPAND(\\)
¬-\-\-¢ISPMODE Session Display#Z¢(80-158)¬-\-\-
%COMMAND ===>¦ZCMD                                            %SCROLL ===>_Z   ¬
Enter an ISPF command, a PDS subcommand or a special control code:
  %LA¬Lista     %1¬Suspend ISPF    %9¬Swap panels   %R¬Recall
  %LV¬Listv     %6¬MEMLIST all     %F¬Find          %X¬Above/Below/All
  %ML¬Memlist   %7¬Output log      %O¬Options       %*¬Memlist *
%&PDSMSG  ----------------------------------------------------------------------
)MODEL
@Z
)INIT
   .HELP  = P86PN1@
   .ZVARS = '(LOGNO ZSCBR PDSLN2)'
   &X = TRUNC(&PDSTNAME,7)
   &LOGNO = .TRAIL
)REINIT
   REFRESH(*)
)PROC
   /* Start NRETRIEV code */
   /* End NRETRIEV code */
   /* Start RefList code */
   VGET (ZRDSN ZRVOL) SHARED
   IF (&ZRDSN ¬= ' ')
     VGET (ZREFVOLM) PROFILE
     IF (&ZREFVOLM = 'ON')
       &VOL1 = &ZRVOL
     ELSE                                                  /* OW15849*/
       &VOL1 = &Z                                          /* OW15849*/
     IF (&VOL1 = ' ')
       &ZCMD = 'C &ZRDSN'
     ELSE
       &ZCMD = 'C &ZRDSN VOL(&VOL1)'
     &ZDSVOL = &VOL1
     &ZRDSN = ' '
     &ZRVOL = ' '
     VPUT (ZRDSN ZRVOL) SHARED
     VPUT (ZDSVOL) SHARED
     VGET (ZELIST) PROFILE
     IF (&ZELIST ¬= 'EXECUTE') .MSG = ISRDS003
   VGET (DSALSEL) SHARED
   IF (&DSALSEL ¬= ' ')
     VGET (DSA1,DSA2,DSA6,DSA7) SHARED
     IF (&DSA7 = ' ')
       &ZCMD = 'C ''&DSA1..&DSA2..&DSA6'''
     ELSE
       &ZCMD = 'C ''&DSA1..&DSA2..&DSA6'';ML &DSA7'
     &DSALSEL = ' '
     VPUT (DSALSEL) SHARED
     VGET (ZELIST) PROFILE
     IF (&ZELIST ¬= 'EXECUTE') .MSG = ISRDS003
   /* End RefList code */

   &PPRE = TRUNC(&ZCMD,3)
   IF (&PPRE = 'c ''' OR           /* cHANGE ' command?     */
       &PPRE = 'C ''')             /* Change ' command?     */
     &PDSN = TRUNC(&ZCMD,2)
     &PDSN = .TRAIL
     &PFC = TRUNC(&PDSN,1)         /* If first character    */
     IF (&PFC = '''')              /*   of DSN is "'" check */
       &PREM = .TRAIL              /*   to see if last "'"  */
       &PREM2 = TRUNC(&PREM,'''')  /*   is missing.         */
       IF (&PREM2 = &PREM)         /* If last "'" missing   */
         &ZCMD = '&ZCMD&PFC'       /*   add it to the end.  */
)END
