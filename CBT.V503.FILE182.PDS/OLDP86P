    &ZPSMLS1 = ''     /*      NAME          */
    &ZPSMLS2 = ''     /*      VERSION       */
    &ZPSMLS3 = ''     /*      CREATED       */
    &ZPSMLS4 = ''     /*      MODIFIED      */
    &ZPSMLS5 = ''     /*      SIZE          */
    &ZPSMLS6 = ''     /*      ID            */
    &ZPSMLS7 = ''     /*      TTR           */
