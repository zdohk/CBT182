./ ADD NAME=P86PNDPS 0113-98282-03344-1407-00129-00038-00000-SBGOLOB
)ATTR DEFAULT(%+_)
    % TYPE(TEXT) INTENS(HIGH) COLOR(WHITE) SKIP(ON)
    ¢ TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW)
    + TYPE(TEXT) INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    ¬ TYPE(OUTPUT) INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    _ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED)
    ¦ TYPE(INPUT) INTENS(HIGH) CAPS(OFF)
    @ TYPE(OUTPUT) INTENS(LOW)
    } TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW) SKIP(ON) HILITE(USCORE)
    $ TYPE(PS)
)BODY EXPAND(\\)
+-\-\-¢MEMLIST Load Member List 3+-\-\-
%COMMAND ===>¦ZCMD                                            %SCROLL ===>_Z   +
Enter an ISPF command, a PDS subcommand or a special control code:
 % 8+View log  %1+Suspend ISPF   %6+MEMLIST all   %F+Find    % R+Recall
 %LA+Lista     %4+Sublist =      %7+Output table  %L+Locate  %SO+Sort
 %LV+Listv     %5+Check aliases  %9+Swap panels   %O+Options  %X+Above/Below/All
%&PDSMSG  ----------------------------------------------------------------------
}CMD $NAME    }DATA    +ALIASOF $LEN/LKED+  --  ATTRIBUTES   -- AC MODE  MAIN
+                       MATCH   $LENGTH  $TTR      +ENTRY     SSI
)MODEL
%---- -------- -------- -------- -------- ---- ---- ---- ---- ---- ---- --------
_Z   ¬Z       _Z       @Z       @Z         @Z   @Z   @Z   @Z    @Z@Z   @Z
+                      @Z       @Z       @Z        @Z        @Z        +
)INIT
   .HELP  = P86PND@
   .ZVARS = '(ZSCBR MEMLOPT  MEMLNAME MEMLDATA MEMLALIS MEMLLDAX +
            MEMLATT1 MEMLATT2 MEMLDC   MEMLTEST MEMLAPFX MEMLMODE MEMLMNAM +
            MEMLMTTR MEMLLENG MEMLTTR MEMLENTY MEMLSSI)'
)REINIT
   REFRESH(*)
)PROC
IF (&ZCMD = 'NAME')
  IF (&ZPSMLL1 = '')
    &ZCMD = 'SORT NAME ASCENDING'
    &ZPSMLL1 = ''     /*      NAME          */
    &ZPSMLL2 = ''     /*      CREATED       */
    &ZPSMLL3 = ''     /*      LENGTH        */
    &ZPSMLL4 = ''     /*      TTR           */
    &ZPSMLL1 = 1
  ELSE
    &ZCMD = 'SORT NAME DESCENDING'
    &ZPSMLL1 = ''
IF (&ZCMD = 'CREATED')
  IF (&ZPSMLL2 = '')
    &ZCMD = 'SORT LENLKED  DESCENDING'
    &ZPSMLL1 = ''     /*      NAME          */
    &ZPSMLL2 = ''     /*      CREATED       */
    &ZPSMLL3 = ''     /*      LENGTH        */
    &ZPSMLL4 = ''     /*      TTR           */
    &ZPSMLL2 = 1
  ELSE
    &ZCMD = 'SORT LENLKED ASCENDING'
    &ZPSMLL2 = ''
IF (&ZCMD = 'LENGTH')
  IF (&ZPSMLL3 = '')
    &ZCMD = 'SORT LENGTH DESCENDING'
    &ZPSMLL1 = ''     /*      NAME          */
    &ZPSMLL2 = ''     /*      CREATED       */
    &ZPSMLL3 = ''     /*      LENGTH        */
    &ZPSMLL4 = ''     /*      TTR           */
    &ZPSMLL3 = 1
  ELSE
    &ZCMD = 'SORT LENGTH ASCENDING'
    &ZPSMLL3 = ''
IF (&ZCMD = 'TTR')
  IF (&ZPSMLL4 = '')
    &ZCMD = 'SORT TTR ASCENDING'
    &ZPSMLL1 = ''     /*      NAME          */
    &ZPSMLL2 = ''     /*      CREATED       */
    &ZPSMLL3 = ''     /*      LENGTH        */
    &ZPSMLL4 = ''     /*      TTR           */
    &ZPSMLL4 = 1
  ELSE
    &ZCMD = 'SORT TTR DESCENDING'
    &ZPSMLL4 = ''

/* Start NRETRIEV code */
/* End NRETRIEV code */

/* Start RefList code */
VGET (ZRDSN ZRVOL) SHARED
IF (&ZRDSN ¬= ' ')
  VGET (ZREFVOLM) PROFILE
  IF (&ZREFVOLM = 'ON')
    &VOL1 = &ZRVOL
  ELSE                                                  /* OW15849*/
    &VOL1 = &Z                                          /* OW15849*/
  IF (&VOL1 = ' ')
    &ZCMD = 'C &ZRDSN'
  ELSE
    &ZCMD = 'C &ZRDSN VOL(&VOL1)'
  &ZDSVOL = &VOL1
  &ZRDSN = ' '
  &ZRVOL = ' '
  VPUT (ZRDSN ZRVOL) SHARED
  VPUT (ZDSVOL) SHARED
  VGET (ZELIST) PROFILE
  IF (&ZELIST ¬= 'EXECUTE') .MSG = ISRDS003
VGET (DSALSEL) SHARED
IF (&DSALSEL ¬= ' ')
  VGET (DSA1,DSA2,DSA6,DSA7) SHARED
  IF (&DSA7 = ' ')
    &ZCMD = 'C ''&DSA1..&DSA2..&DSA6'''
  ELSE
    &ZCMD = 'C ''&DSA1..&DSA2..&DSA6'';ML &DSA7'
  &DSALSEL = ' '
  VPUT (DSALSEL) SHARED
  VGET (ZELIST) PROFILE
  IF (&ZELIST ¬= 'EXECUTE') .MSG = ISRDS003
/* End RefList code */

&PPRE = TRUNC(&ZCMD,3)
IF (&PPRE = 'c ''' OR           /* cHANGE ' command?     */
    &PPRE = 'C ''')             /* Change ' command?     */
  &PDSN = TRUNC(&ZCMD,2)
  &PDSN = .TRAIL
  &PFC = TRUNC(&PDSN,1)         /* If first character    */
  IF (&PFC = '''')              /*   of DSN is "'" check */
    &PREM = .TRAIL              /*   to see if last "'"  */
    &PREM2 = TRUNC(&PREM,'''')  /*   is missing.         */
    IF (&PREM2 = &PREM)         /* If last "'" missing   */
      &ZCMD = '&ZCMD&PFC'       /*   add it to the end.  */
)PNTS
FIELD(ZPS00001) VAR(ZCMD) VAL(NAME)
FIELD(ZPS00002) VAR(ZCMD) VAL(CREATED)
FIELD(ZPS00003) VAR(ZCMD) VAL(LENGTH)
FIELD(ZPS00004) VAR(ZCMD) VAL(TTR)
)END
