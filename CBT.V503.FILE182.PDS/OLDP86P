/* End NRETRIEV code */

/* Start RefList code */
VGET (ZRDSN ZRVOL) SHARED
IF (&ZRDSN ¬= ' ')
  VGET (ZREFVOLM) PROFILE
  IF (&ZREFVOLM = 'ON')
    &VOL1 = &ZRVOL
  ELSE                                                  /* OW15849*/
    &VOL1 = &Z                                          /* OW15849*/
  IF (&VOL1 = ' ')
    &ZCMD = 'C &ZRDSN'
  ELSE
