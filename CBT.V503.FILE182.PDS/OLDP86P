  VPUT (ZDSVOL) SHARED
  VGET (ZELIST) PROFILE
  IF (&ZELIST ¬= 'EXECUTE') .MSG = ISRDS003
VGET (DSALSEL) SHARED
IF (&DSALSEL ¬= ' ')
  VGET (DSA1,DSA2,DSA6,DSA7) SHARED
  IF (&DSA7 = ' ')
