         MVI   16(R5),C' '              CLEAN OUT GARBAGE               01159
         TM    DIRFLAG,X'80'            ALIAS?                          01160
         BZ    *+10                     NO, SKIP THE '-A' SET           01161
         MVC   16(L'IDALIAS,R5),IDALIAS SET ALIAS                       01162
