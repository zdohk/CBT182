IFDATESB IKJSUBF
         AIF   ('&ISODT'  EQ 'YES').ISOH3                     DRK NOV01
IFDATESU IKJIDENT 'DATE',                                              $
               RANGE,FIRST=ANY,OTHER=ANY,CHAR,                         $
               PROMPT='DATE CHANGED',MAXLNTH=08,VALIDCK=VRTDATE,       $
               HELP=('THE DATE TO USE -- LIKE 12/15/86 OR 86349')
         AGO   .ISOH3X                                        DRK NOV01
.ISOH3   ANOP                                                 DRK NOV01
IFDATESU IKJIDENT 'DATE',                                              $
               RANGE,FIRST=ANY,OTHER=ANY,CHAR,                         $
               PROMPT='DATE CHANGED',MAXLNTH=08,VALIDCK=VRTDATE,       $
               HELP=('THE DATE TO USE -- LIKE 86/12/15 OR 86349')
.ISOH3X  ANOP                                                 DRK NOV01
