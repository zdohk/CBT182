               FIRST=ALPHANUM,OTHER=ALPHANUM,                          $
               PROMPT='DEFAULT VOLUME NAME',                           $
               HELP=('DEFAULT NAME FOR THE VOLUME PARAMETER')
         IKJENDP
         SPACE 2
