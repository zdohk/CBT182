SYMDEL   DSECT ,           Z/XDC DELETE SYMBOLS               DRK DEC09
         SPACE 5
         TITLE 'P D S  --  PDS DYNAMIC AREA                          '
         COPY  #MDSECT
SYMNODEL DSECT ,           Z/XDC DELETE SYMBOLS               DRK DEC09
         SPACE 5
         TITLE 'P D S  --  THE DEFAULT PARAMETER LIST                '
         IKJDFPL
         SPACE 5
         TITLE 'P D S  --  THE DEFAULT PARAMETER BLOCK               '
         IKJDFPB
         SPACE 5
         TITLE 'P D S  --  PARSE PARAMETER LIST                      '
         IKJPPL
         SPACE 5
         TITLE 'P D S  --  THE USER PROFILE TABLE                    '
         IKJUPT
         SPACE 5
         TITLE 'P D S  --  COMMUNICATIONS VECTOR TABLE               '
         CVT   DSECT=YES
         SPACE 5
         TITLE 'P D S  --  TSO/E VECTOR TABLE                        '
         IKJTSVT                                              DRK JUL17
         SPACE 5
         AIF   (D'TSVTUMXL).Z23C
TSVTUMXL EQU   TSVT+X'06',1
*PSCBUID8 EQU   PSCB+X'54',8
*PSCBU8L  EQU   PSCB+X'20',1
         AIF   (D'UPTPREF8).Z23C                              DRK SEP17
UPTPREF8 EQU    UPT+X'21',8
UPTPRF8L EQU    UPT+X'29',1
.Z23C    ANOP
.NOGEN   END
