//* HIGH LEVEL ASSEMBLER
//* GENERATE ADATA FOR Z/XDC SOURCE LEVEL DEBUGGING
//PDSGEN   PROC  MEMBER=MISSING,
//             INSTLIB='PDS.INSTALL.LIBRARY'        <<=== INSTALL LIB
//* INVOKE HIGH LEVEL ASSEMBLER WITH ADATA OPTION
//ASM      EXEC  PGM=ASMA90,REGION=32M,
//             PARM=(NORLD,RENT,TERM,OBJ,ADATA,
//             'XREF(SHORT,UNREFS),SIZE(MAX,ABOVE)',
//             'FLAG(PAGE0)')
//SYSADATA DD  DSN=PDS86.ADATA,DISP=(,CATLG,DELETE),
//             UNIT=SYSALLDA,SPACE=(CYL,(50,10))
//SYSLIN   DD  UNIT=VIO,SPACE=(TRK,(5,5)),DISP=(MOD,PASS),DSN=&&X,
//             DCB=BLKSIZE=3120
//SYSPRINT DD  SYSOUT=*
//SYSTERM  DD  SYSOUT=*
//SYSIN    DD  DISP=SHARE,DSN=&INSTLIB.(&MEMBER)
//SYSLIB   DD  DSN=SYS1.MODGEN,DISP=SHARE
//         DD  DSN=SYS1.MACLIB,DISP=SHARE
//         DD  DSN=&INSTLIB,DISP=SHARE
//         PEND
//IDCAMS   EXEC  PGM=IDCAMS
//SYSPRINT DD SYSOUT=*
 DELETE PDS86.ADATA
//*
//PDSMAIN  EXEC  PDSGEN,MEMBER=@PDSMAIN
//*
//SUBS     EXEC  PDSGEN,MEMBER=@SUBS
//SYSADATA DD  DISP=MOD
//*
//PARSE    EXEC  PDSGEN,MEMBER=@PARSE
//SYSADATA DD  DISP=MOD
//*
//DIALOG   EXEC  PDSGEN,MEMBER=@DIALOG
//SYSADATA DD  DISP=MOD
//*
//LKED     EXEC  PGM=IEWL,REGION=32M,
//             COND=((0,LT,PDSMAIN.ASM),
//             (0,LT,SUBS.ASM),
//             (0,LT,PARSE.ASM),
//             (0,LT,DIALOG.ASM)),
//             PARM='TEST,MAP,RENT,REUS,REFR,NCAL'
//SYSPRINT DD  SYSOUT=*
//IN       DD  DSN=&&X,DISP=(OLD,DELETE)
//SYSLMOD  DD  DISP=SHARE,DSN=LINKLIST.LIB          <<=== LINK TARGET
//SYSLIN   DD  *
  INCLUDE  IN
  ENTRY    PDSMAIN
  ORDER    PDSMAIN(P)
  ALIAS    PDS                                      <<=== FOR DSLIST
  ALIAS    PDSE                                     <<=== FOR DSLIST
  NAME     PDS86(R)                                 <<=== LOADMOD NAME
